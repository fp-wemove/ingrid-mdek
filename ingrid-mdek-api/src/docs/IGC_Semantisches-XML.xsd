        <xs:element minOccurs="0" ref="udk:country"/>
        <xs:element minOccurs="0" ref="udk:state"/>
        <xs:element minOccurs="0" ref="udk:date-of-last-modification"/>
        <xs:element minOccurs="0" ref="udk:date-of-creation"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Katalog
    Tabelle.Feld   : t01_object.cat_id
  -->

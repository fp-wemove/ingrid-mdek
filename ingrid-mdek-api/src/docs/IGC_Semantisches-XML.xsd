        <xs:element ref="udk:creator-identifier"/>
        <xs:element ref="udk:modificator-identifier"/>
        <xs:element ref="udk:object-class"/>
        <xs:element ref="udk:title"/>
        <xs:element minOccurs="0" ref="udk:abstract"/>
        <xs:element minOccurs="0" ref="udk:language-of-resource"/>
        <xs:element minOccurs="0" ref="udk:language-of-record"/>
        <xs:element minOccurs="0" ref="udk:date-of-last-modification"/>
        <xs:element minOccurs="0" ref="udk:date-of-creation"/>
        <xs:element minOccurs="0" ref="udk:original-control-identifier"/>

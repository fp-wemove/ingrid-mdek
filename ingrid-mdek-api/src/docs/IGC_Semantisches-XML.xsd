  <xs:element name="dataset-topic-category">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="udk:attlist.udk.dataset-topic-category"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Themenkategorie
    Tabelle.Feld   : T011_obj_geo_topic_cat.dataset_topic_category

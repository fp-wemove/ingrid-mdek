        <xs:element ref="udk:general"/>
        <xs:element minOccurs="0" ref="udk:technical-domain"/>
        <xs:element ref="udk:additional-information"/>
        <xs:element ref="udk:spatial-domain"/>
        <xs:element minOccurs="0" ref="udk:temporal-domain"/>
        <xs:element minOccurs="0" ref="udk:subject-terms"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:available-linkage"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:related-address"/>
        <xs:element minOccurs="0" ref="udk:parent-data-source"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:child-data-source"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:link-data-source"/>

  <!--
    Fachlicher Name: Erscheinungsjahr
    Tabelle.Feld   : T011_obj_literatur.publish_year
  -->
  <xs:element name="year" type="xs:string"/>
  <!--
    Fachlicher Name: ISBN-Nr.
    Tabelle.Feld   : T011_obj_literatur.isbn
  -->
  <xs:element name="isbn" type="xs:string"/>
  <!--
    Fachlicher Name: Basisdaten
    Tabelle.Feld   : T011_obj_literatur.base
  -->
  <xs:element name="source" type="xs:string"/>
  <!--
    Fachlicher Name: Dokumententyp
    Tabelle.Feld   : T011_obj_literatur.typ
  -->
  <xs:element name="type-of-document" type="xs:string"/>
  <!--
    Fachlicher Name: Herausgeber
    Tabelle.Feld   : T011_obj_literatur.

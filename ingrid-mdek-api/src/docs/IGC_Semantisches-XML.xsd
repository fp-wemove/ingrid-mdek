<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="udk:udk" xmlns:udk="udk:udk">
  <!-- Version des Austauschformats -->
  <xs:attributeGroup name="attlist.udk">
    <xs:attribute name="exchange-format" default="1.0"/>
  </xs:attributeGroup>
  <xs:element name="udk">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:catalogue"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:data-source"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:address"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:data-source-relation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:data-source-address-relation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:address-relation"/>
        <xs:element minOccurs="0" ref="udk:data-model-extension"/>
      </xs:sequence>
      <xs:attributeGroup ref="udk:attlist.udk"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="catalogue">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="udk:catalogue-identifier"/>
        <xs:element ref="udk:catalogue-name"/>
        <xs:element minOccurs="0" ref="udk:partner-name"/>
        <xs:element minOccurs="0" ref="udk:provider-name"/>
        <xs:element minOccurs="0" ref="udk:country"/>
        <xs:element minOccurs="0" ref="udk:language"/>
        <xs:element minOccurs="0" ref="udk:geo-location"/>
        <xs:element minOccurs="0" ref="udk:workflow-control"/>
        <xs:element minOccurs="0" ref="udk:expiry-duration"/>
        <xs:element minOccurs="0" ref="udk:date-of-creation"/>
        <xs:element minOccurs="0" ref="udk:modificator-identifier"/>
        <xs:element minOccurs="0" ref="udk:date-of-last-modification"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Katalog Uuid
    Tabelle.Feld   : T03_catalogue.cat_uuid
  -->
  <xs:element name="catalogue-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Erstellt von
    Tabelle.Feld   : object_comment.create_uuid
  -->
  <xs:element name="creator-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Zuletzt geaendert von
    Tabelle.Feld   : T03_catalogue.mod_uuid
  -->
  <xs:element name="modificator-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Katalog Name
    Tabelle.Feld   : T03_catalogue.cat_name 
  -->
  <xs:element name="catalogue-name" type="xs:string"/>
  <!--
    Fachlicher Name: Partner Name
    Tabelle.Feld   : T03_catalogue.partner_name
  -->
  <xs:element name="partner-name" type="xs:string"/>
  <!--
    Fachlicher Name: Anbieter Name
    Tabelle.Feld   : T03_catalogue.provider_name
  -->
  <xs:element name="provider-name" type="xs:string"/>
  <!--
    Fachlicher Name: Land
    Tabelle.Feld   : T03_catalogue.country_code
  -->
  <xs:element name="country">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.country"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: ??
    Tabelle.Feld   : T03_catalogue.state
  -->
  <xs:element name="state" type="xs:string"/>
  <!--
    Fachlicher Name: Workflow Kontrolle
    Tabelle.Feld   : T03_catalogue.workflow_control
  -->
  <xs:element name="workflow-control">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.enabled"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Ablaufdauer
    Tabelle.Feld   : T03_catalogue.expiry_duration
  -->
  <xs:element name="expiry-duration" type="xs:string"/>
  <!--
    Fachlicher Name: Zuletzt geaendert am
    Tabelle.Feld   : T01_object.mod_time
  -->
  <xs:element name="date-of-last-modification" type="xs:string"/>
  <!--
    Fachlicher Name: Erstellt am
    Tabelle.Feld   : T01_object.create_time
  -->
  <xs:element name="date-of-creation" type="xs:string"/>
  <xs:element name="data-source">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="udk:general"/>
        <xs:element minOccurs="0" ref="udk:technical-domain"/>
        <xs:element ref="udk:additional-information"/>
        <xs:element ref="udk:spatial-domain"/>
        <xs:element minOccurs="0" ref="udk:temporal-domain"/>
        <xs:element minOccurs="0" ref="udk:subject-terms"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:available-linkage"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:related-address"/>
        <xs:element minOccurs="0" ref="udk:parent-data-source"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:child-data-source"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:link-data-source"/>
      </xs:sequence>
      <xs:attributeGroup ref="udk:attlist.udk.data-source"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.udk.data-source">
    <xs:attribute name="server-id"/>
  </xs:attributeGroup>
  <xs:element name="general">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="udk:object-identifier"/>
        <xs:element ref="udk:catalogue-identifier"/>
        <xs:element ref="udk:modificator-identifier"/>
        <xs:element ref="udk:responsible-identifier"/>
        <xs:element ref="udk:object-class"/>
        <xs:element ref="udk:title"/>
        <xs:element minOccurs="0" ref="udk:abstract"/>
        <xs:element minOccurs="0" ref="udk:date-of-last-modification"/>
        <xs:element minOccurs="0" ref="udk:date-of-creation"/>
        <xs:element minOccurs="0" ref="udk:original-control-identifier"/>
        <xs:element minOccurs="0" ref="udk:general-additional-values"/>
        <xs:element ref="udk:metadata"/>
        <xs:element minOccurs="0" ref="udk:dataset-alternate-name"/>
        <xs:element minOccurs="0" ref="udk:dataset-character-set"/>
        <xs:element ref="udk:topic-categories"/>
        <xs:element minOccurs="0" ref="udk:env-information"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Objekt Uuid
    Tabelle.Feld   : T01_object.obj_uuid
  -->
  <xs:element name="object-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Verantwortlicher
    Tabelle.Feld   : T01_object.responsible_uuid
  -->
  <xs:element name="responsible-identifier" type="xs:string"/>
  <!--
    Fachlicher Name:
    Tabelle.Feld   : T01_object.obj_class
  -->
  <xs:element name="object-class">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.object-class"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Objektname
    Tabelle.Feld   : T01_object.obj_name
  -->
  <xs:element name="title" type="xs:string"/>
  <!--
    Fachlicher Name: Beschreibung
    Tabelle.Feld   : T01_object.obj_descr
  -->
  <xs:element name="abstract" type="xs:string"/>
  <!--
    Fachlicher Name: Original ID
    Tabelle.Feld   : T01_object.org_obj_id
  -->
  <xs:element name="original-control-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Zusatzfelder
    Tabelle   : t08_attr*
  -->
  <xs:element name="general-additional-values">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:general-additional-value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Zusatzfeld (generische Attribute)
    Tabelle   : t08_attr_type, t08_attr
  -->
  <xs:element name="general-additional-value">
    <xs:complexType>
      <xs:sequence>
        <!-- -TODO: field-name als ID ? oder numerische ID nehmen, damit unterschiedliche Felder mit gleichem Namen moeglich ? --> 
        <xs:element ref="udk:field-name"/>
        <xs:element ref="udk:field-value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Attributname
    Tabelle.Feld   : t08_attr_type.name
  -->
  <xs:element name="field-name" type="xs:string"/>
  <!--
    Fachlicher Name: Attributwert
    Tabelle.Feld   : t08_attr.data
  -->
  <xs:element name="field-value" type="xs:string"/>
  <!-- Neu in T01_object, Elemente von general -->
  <xs:element name="metadata">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="udk:metadata-character-set"/>
      </xs:sequence>
      <xs:attributeGroup ref="udk:attlist.udk.metadata"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Name des Metadatenstandards
    Tabelle.Feld   : statisch T01_object.metadata_standard_name
    Fachlicher Name: Version des Metadatenstandards
    Tabelle.Feld   : statisch T01_object.metadata_standard_version
  -->
  <xs:attributeGroup name="attlist.udk.metadata">
    <xs:attribute name="metadata-standard-name" default="ISO-19115"/>
    <xs:attribute name="metadata-standard-version" default="1.0"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Zeichensatz des Metadatensatzes
    Tabelle.Feld   : T01_object.metadata_character_set
  -->
  <xs:element name="metadata-character-set">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.metadata-character-set"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name:
    Tabelle.Feld   : T01_object.metadata_character_set
  -->
  <xs:attributeGroup name="attlist.udk.metadata-character-set">
    <xs:attribute name="domain-id" use="required"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Kurzbeschreibung
    Tabelle.Feld   : T01_object.dataset_alternate_name
  -->
  <xs:element name="dataset-alternate-name" type="xs:string"/>
  <!--
    Fachlicher Name: Zeichensatz des Datensatzes
    Tabelle.Feld   : T01_object.dataset_character_set
  -->
  <xs:element name="dataset-character-set">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.dataset-character-set"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name:
    Tabelle.Feld   : T01_object.dataset_character_set
  -->
  <xs:attributeGroup name="attlist.udk.dataset-character-set">
    <xs:attribute name="domain-id" use="required"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Themenkategorien eines Objektes
    Tabelle.Feld   : t011_obj_topic_cat
  -->
  <xs:element name="topic-categories">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="udk:topic-category"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Themenkategorie eines Objektes
    Tabelle.Feld   : t011_obj_topic_cat
  -->
  <xs:element name="topic-category">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.topic-category"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Themenkategorie eines Objektes
    Tabelle.Feld   : t011_obj_topic_cat.topic_category
  -->
  <xs:attributeGroup name="attlist.udk.topic-category">
    <xs:attribute name="key" use="required"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Katalog Umweltthemen Infos (Katalogseite)
    Tabellen   : T01_object, T0114_env_topic, T0114_env_category
  -->
  <xs:element name="env-information">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:env-topic"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:env-category"/>
      </xs:sequence>
      <xs:attributeGroup ref="udk:attlist.udk.env-information"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Umweltthema (Katalogseite)
    Tabelle.Feld   : T0114_env_topic
  -->
  <xs:element name="env-topic">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.env-topic"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Umweltthema Schlüssel (Katalogseite)
    Tabelle.Feld   : T0114_env_topic.topic_key
  -->
  <xs:attributeGroup name="attlist.udk.env-topic">
    <xs:attribute name="key" use="required"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Umweltkategorie (Katalogseite)
    Tabelle.Feld   : t0114_env_category
  -->
  <xs:element name="env-category">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.env-category"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Umweltkategorie Schlüssel (Katalogseite)
    Tabelle.Feld   : t0114_env_category.cat_key
  -->
  <xs:attributeGroup name="attlist.udk.env-category">
    <xs:attribute name="key" use="required"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Als Katalogseite in Umweltthemen anzeigen
    Tabelle.Feld   : T01_object.is_catalog_data
  -->
  <xs:attributeGroup name="attlist.udk.env-information">
    <xs:attribute name="is-catalog" default="N">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="Y"/>
          <xs:enumeration value="N"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Objektklassen
  -->
  <xs:element name="technical-domain">
    <xs:complexType>
      <xs:choice minOccurs="0">
        <xs:element ref="udk:dataset"/>
        <xs:element ref="udk:service"/>
        <xs:element ref="udk:document"/>
        <xs:element ref="udk:map"/>
        <xs:element ref="udk:project"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Datensammlung /Datenbank
    Tabelle   : t011_obj_data*
  -->
  <xs:element name="dataset">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="udk:description-of-tech-domain"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:dataset-parameter"/>
        <xs:element minOccurs="0" ref="udk:method"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Erl㴴erung zum Fachbezug
    Tabelle.Feld   : T011_obj_*.description
  -->
  <xs:element name="description-of-tech-domain" type="xs:string"/>
  <!--
    Fachlicher Name: Inhalte / Ergaenzende Angaben der Datensammlung/Datenbank
    Tabelle   : T011_obj_data_para
  -->
  <xs:element name="dataset-parameter">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="udk:parameter"/>
        <xs:element minOccurs="0" ref="udk:supplemantary-information"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Inhalte der Datensammlung/Datenbank
    Tabelle.Feld   : T011_obj_data_para.parameter
  -->
  <xs:element name="parameter" type="xs:string"/>
  <!--
    Fachlicher Name: Ergaenzende Angaben (Hinweis: Einheit der Parameter)
    Tabelle.Feld   : T011_obj_data_para.unit
  -->
  <xs:element name="supplemantary-information" type="xs:string"/>
  <!--
    Fachlicher Name: Methode/Datengrundlage
    Tabelle.Feld   : T011_obj_data.base
  -->
  <xs:element name="method" type="xs:string"/>
  <!--
    Fachlicher Name: Dienst / Anwendung / Informationssystem
    Tabelle   : t011_obj_serv*
  -->
  <xs:element name="service">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="udk:description-of-tech-domain"/>
        <xs:element maxOccurs="unbounded" ref="udk:service-type-key"/>
        <xs:element maxOccurs="unbounded" ref="udk:service-classification-key"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:publication-scale"/>
        <xs:element minOccurs="0" ref="udk:system-history"/>
        <xs:element minOccurs="0" ref="udk:database-of-system"/>
        <xs:element minOccurs="0" ref="udk:system-environment"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:service-version"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:service-operation"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Historie
    Tabelle.Feld   : T011_obj_serv.history
  -->
  <xs:element name="system-history" type="xs:string"/>
  <!--
    Fachlicher Name: Basisdaten
    Tabelle.Feld   : T011_obj_serv.base
  -->
  <xs:element name="database-of-system" type="xs:string"/>
  <!--
    Fachlicher Name: Service-Klassifikation
    Tabelle.Feld   : T011_obj_serv.type_key
  -->
  <xs:element name="service-classification-key" type="xs:string"/>
  <!--
    Fachlicher Name: Systemumgebung
    Tabelle.Feld   : T011_obj_serv.environment
  -->
  <xs:element name="system-environment" type="xs:string"/>
  <!--
    Fachlicher Name: Service-Typ (INSPIRE Classification of spatial data services)
    Tabelle.Feld   : T011_obj_serv_type.serv_type_key
  -->
  <xs:element name="service-type-key" type="xs:string"/>
  <!--
    Fachlicher Name: Version des Service
    Tabelle.Feld   : T011_obj_serv_version.version
  -->
  <xs:element name="service-version" type="xs:string"/>
  <!--
    Fachlicher Name: Operation
    Tabelle   : T011_obj_serv_op*
  -->
  <xs:element name="service-operation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="udk:description-of-operation"/>
        <xs:element maxOccurs="unbounded" ref="udk:platform"/>
        <xs:element maxOccurs="unbounded" ref="udk:connection-point"/>
        <xs:element minOccurs="0" ref="udk:invocation-name"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:parameter-of-operation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:depends-on"/>
      </xs:sequence>
      <xs:attributeGroup ref="udk:attlist.udk.service-operation"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Operation-Namensschluessel / -Name
    Tabelle.Feld   : T011_obj_serv_operation.name_key
    Tabelle.Feld   : T011_obj_serv_operation.name_value
  -->
  <xs:attributeGroup name="attlist.udk.service-operation">
    <xs:attribute name="name-key" use="required"/>
    <xs:attribute name="name"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Operationbeschreibung
    Tabelle.Feld   : T011_obj_serv_operation.descr
  -->
  <xs:element name="description-of-operation" type="xs:string"/>
  <!--
    Fachlicher Name: Aufruf
    Tabelle.Feld   : T011_obj_serv_operation.invocation_name
  -->
  <xs:element name="invocation-name" type="xs:string"/>
  <!--
    Fachlicher Name: Platform
    Tabelle.Feld   : T011_obj_serv_op_platform.platform
  -->
  <xs:element name="platform" type="xs:string"/>
  <!--
    Fachlicher Name: Parameter der Operation
    Tabelle   : T011_obj_serv_op_para
  -->
  <xs:element name="parameter-of-operation">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="udk:description-of-parameter"/>
      </xs:sequence>
      <xs:attributeGroup ref="udk:attlist.udk.parameter-of-operation"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Beschreibung des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.descr
  -->
  <xs:element name="description-of-parameter" type="xs:string"/>
  <!--
    Fachlicher Name: Attribute des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.name
    Tabelle.Feld   : T011_obj_serv_op_para.optional
    Tabelle.Feld   : T011_obj_serv_op_para.repeatability
    Tabelle.Feld   : T011_obj_serv_op_para.direction
  -->
  <xs:attributeGroup name="attlist.udk.parameter-of-operation">
    <xs:attribute name="name" use="required"/>
    <xs:attribute name="optional" use="required"/>
    <xs:attribute name="repeatability" use="required"/>
    <xs:attribute name="direction"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Abhängigkeiten
    Tabelle.Feld   : T011_obj_serv_op_depends.depends_on
  -->
  <xs:element name="depends-on" type="xs:string"/>
  <!--
    Fachlicher Name: Zugriffsadresse
    Tabelle.Feld   : t011_obj_serv_op_connpoint.connect_point
  -->
  <xs:element name="connection-point" type="xs:string"/>
  <!--
    Fachlicher Name: Gutachten / Bericht / Literatur
    Tabelle   : t011_obj_literature
  -->
  <xs:element name="document">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="udk:description-of-tech-domain"/>
        <xs:element minOccurs="0" ref="udk:publisher"/>
        <xs:element minOccurs="0" ref="udk:publishing-place"/>
        <xs:element minOccurs="0" ref="udk:year"/>
        <xs:element minOccurs="0" ref="udk:isbn"/>
        <xs:element minOccurs="0" ref="udk:source"/>
        <xs:element minOccurs="0" ref="udk:type-of-document"/>
        <xs:element minOccurs="0" ref="udk:editor"/>
        <xs:element minOccurs="0" ref="udk:author"/>
        <xs:element minOccurs="0" ref="udk:additional-bibliographic-info"/>
        <xs:element minOccurs="0" ref="udk:location"/>
        <xs:element minOccurs="0" ref="udk:pages"/>
        <xs:element minOccurs="0" ref="udk:volume"/>
        <xs:element minOccurs="0" ref="udk:published-in"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Verlag
    Tabelle.Feld   : t011_obj_literature.publishing
  -->
  <xs:element name="publisher" type="xs:string"/>
  <!--
    Fachlicher Name: Erscheinungsort
    Tabelle.Feld   : t011_obj_literature.publish_loc
  -->
  <xs:element name="publishing-place" type="xs:string"/>
  <!--
    Fachlicher Name: Erscheinungsjahr
    Tabelle.Feld   : t011_obj_literature.publish_year
  -->
  <xs:element name="year" type="xs:string"/>
  <!--
    Fachlicher Name: ISBN-Nr.
    Tabelle.Feld   : t011_obj_literature.isbn
  -->

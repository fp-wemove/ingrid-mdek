<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="udk:udk" xmlns:udk="udk:udk">
  <xs:element name="udk">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:catalogue"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:data-source"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:address"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:data-source-relation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:data-source-address-relation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:address-relation"/>

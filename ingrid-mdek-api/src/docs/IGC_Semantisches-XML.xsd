  <xs:element name="pages" type="xs:string"/>
  <!--
    Fachlicher Name: Band, Heft
    Tabelle.Feld   : T011_obj_literatur.volume
  -->
  <xs:element name="volume" type="xs:string"/>
  <!--
    Fachlicher Name: Erschienen in
    Tabelle.Feld   : T011_obj_literatur.publish_in
  -->
  <xs:element name="published-in" type="xs:string"/>
  <xs:element name="map">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="udk:data"/>
        <xs:element minOccurs="0" ref="udk:resolution"/>
        <xs:element minOccurs="0" ref="udk:co-ordinate-system"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:publication-scale"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:key-catalogue"/>
        <xs:element minOccurs="0" ref="udk:degree-of-record"/>
        <xs:element minOccurs="0" ref="udk:method-of-production"/>
        <xs:element minOccurs="0" ref="udk:technical-base"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:symbol-catalogue"/>
        <xs:element maxOccurs="unbounded" ref="udk:dataset-topic-category"/>
        <xs:element ref="udk:hierarchy-level"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:spatial-representation-type"/>
        <xs:element minOccurs="0" ref="udk:vector-format"/>
        <xs:element minOccurs="0" ref="udk:reference-system-id"/>
        <xs:element minOccurs="0" ref="udk:pos-accuracy-vertical"/>
        <xs:element minOccurs="0" ref="udk:keyc-incl-w-dataset"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:feature-type"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Datengrundlage, Beschreibung der Datengrundlage
    Tabelle.Feld   : T011_obj_geo.data_base
  -->
  <xs:element name="data" type="xs:string"/>
  <!--
    Fachlicher Name: Lagegenauigkeit, Genauigkeit der Datengewinnung
    Tabelle.Feld   : T011_obj_geo.rec_exact
  -->
  <xs:element name="resolution" type="xs:string"/>
  <!--
    Fachlicher Name: Raum-Bezugssystem, Koordinatensystem der Geoinformation / Karte
    Tabelle.Feld   : T011_obj_geo.coord
  -->
  <xs:element name="co-ordinate-system" type="xs:string"/>
  <!--
    Fachlicher Name:
    Tabelle.Feld   :
  -->
  <xs:element name="publication-scale">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.publication-scale"/>
    </xs:complexType>
  </xs:element>
  <!-- Neu in T011_obj_geo_scale, Attribute von publication-scale -->
  <!--
    Fachlicher Name: Erstellungsma޳tab: Ma޳tab, Erfassungs- bzw. Erstellungsma޳tab
    Tabelle.Feld   : T011_obj_geo_scale.scale
  -->
  <!--
    Fachlicher Name: Erstellungsma޳tab: Bodenaufl�g
    Tabelle.Feld   : T011_obj_geo_scale.resolution_ground
  -->
  <!--
    Fachlicher Name: Erstellungsma޳tab: Scanaufl�g
    Tabelle.Feld   : T011_obj_geo_scale.resolution_scan
  -->
  <xs:attributeGroup name="attlist.udk.publication-scale">
    <xs:attribute name="scale" use="required"/>
    <xs:attribute name="resolution-ground"/>
    <xs:attribute name="resolution-scan"/>
  </xs:attributeGroup>
  <xs:element name="key-catalogue">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.key-catalogue"/>
    </xs:complexType>
  </xs:element>
  <!-- Neu in T011_obj_geo_keyc: -->
  <!--
    Fachlicher Name: Schl�atalog: Titel
    Tabelle.Feld   : T011_obj_geo_keyc.subject_cat
  -->
  <!--
    Fachlicher Name: Schl�atalog: Datum
    Tabelle.Feld   : T011_obj_geo_keyc.key_date
  -->
  <!--
    Fachlicher Name: Schl�atalog: Version
    Tabelle.Feld   : T011_obj_geo_keyc.edition
  -->
  <xs:attributeGroup name="attlist.udk.key-catalogue">
    <xs:attribute name="subject-cat"/>
    <xs:attribute name="key-date"/>
    <xs:attribute name="edition"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Erfassungsgrad, Grad der Erfassung in %
    Tabelle.Feld   : T011_obj_geo.rec_grade
  -->
  <xs:element name="degree-of-record" type="xs:string"/>

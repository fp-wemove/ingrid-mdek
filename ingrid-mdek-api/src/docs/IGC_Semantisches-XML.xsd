<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="igc:igc" xmlns:igc="igc:igc">
  <!-- VERSION DES AUSTAUSCHFORMATES -->
  <xs:attributeGroup name="attlist.igc">
    <xs:attribute name="exchange-format" default="1.0"/>
  </xs:attributeGroup>
  <!-- InGridCatalog (IGC) containing UDK data -->
  <xs:element name="igc">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="igc:catalogue"/>
        <xs:element minOccurs="0" ref="igc:data-sources"/>
        <xs:element minOccurs="0" ref="igc:addresses"/>
        <xs:element minOccurs="0" ref="igc:data-model-extensions"/>
      </xs:sequence>
      <xs:attributeGroup ref="igc:attlist.igc"/>
    </xs:complexType>
  </xs:element>
  <!-- Katalog Daten -->
  <xs:element name="catalogue">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:catalogue-identifier"/>
        <xs:element ref="igc:catalogue-name"/>
        <xs:element minOccurs="0" ref="igc:partner-name"/>
        <xs:element minOccurs="0" ref="igc:provider-name"/>
        <xs:element minOccurs="0" ref="igc:country"/>
        <xs:element minOccurs="0" ref="igc:language"/>
        <xs:element minOccurs="0" ref="igc:geo-location"/>
        <xs:element minOccurs="0" ref="igc:workflow-control"/>
        <xs:element minOccurs="0" ref="igc:expiry-duration"/>
        <xs:element minOccurs="0" ref="igc:date-of-creation"/>
        <xs:element minOccurs="0" ref="igc:modificator-identifier"/>
        <xs:element minOccurs="0" ref="igc:date-of-last-modification"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Katalog Uuid
    Tabelle.Feld   : T03_catalogue.cat_uuid
  -->
  <xs:element name="catalogue-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Zuletzt geaendert von
    Tabelle.Feld   : t03_catalogue.mod_uuid
    Tabelle.Feld   : t01_object.mod_uuid
    Tabelle.Feld   : t02_address.mod_uuid
  -->
  <xs:element name="modificator-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Katalog Name
    Tabelle.Feld   : T03_catalogue.cat_name 
  -->
  <xs:element name="catalogue-name" type="xs:string"/>
  <!--
    Fachlicher Name: Partner Name
    Tabelle.Feld   : T03_catalogue.partner_name
  -->
  <xs:element name="partner-name" type="xs:string"/>
  <!--
    Fachlicher Name: Anbieter Name
    Tabelle.Feld   : T03_catalogue.provider_name
  -->
  <xs:element name="provider-name" type="xs:string"/>
  <!--
    Fachlicher Name: Land
    Tabelle.Feld   : T03_catalogue.country_code
  -->
  <xs:element name="country" type="igc:iso-code"/>
  <!--
    Fachlicher Name: Sprache
    Tabelle.Feld   : T03_catalogue.language_code
  -->
  <xs:element name="language" type="igc:iso-code"/>
  <!--
    Fachlicher Name: Workflow Kontrolle
    Tabelle.Feld   : T03_catalogue.workflow_control
  -->
  <xs:element name="workflow-control" type="igc:igc-boolean"/>
  <!--
    Fachlicher Name: Ablaufdauer
    Tabelle.Feld   : T03_catalogue.expiry_duration
  -->
  <xs:element name="expiry-duration" type="xs:string"/>
  <!--
    Fachlicher Name: Zuletzt geaendert am
    Tabelle.Feld   : T03_catalogue.mod_time
    Tabelle.Feld   : T01_object.mod_time
    ...
  -->
  <xs:element name="date-of-last-modification" type="xs:string"/>
  <!--
    Fachlicher Name: Erstellt am
    Tabelle.Feld   : T01_object.create_time
  -->
  <xs:element name="date-of-creation" type="xs:string"/>
  <!--
    Fachlicher Name: Objekt MetadatensÃ¤tze 
  -->
  <xs:element name="data-sources">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:data-source"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Objekt Metadatensatz 
  -->
  <xs:element name="data-source">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:general"/>
        <xs:element ref="igc:technical-domain"/>
        <xs:element ref="igc:additional-information"/>
        <xs:element ref="igc:spatial-domain"/>
        <xs:element minOccurs="0" ref="igc:temporal-domain"/>
        <xs:element minOccurs="0" ref="igc:subject-terms"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:available-linkage"/>
        <xs:element minOccurs="0" ref="igc:parent-data-source"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:related-address"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:link-data-source"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="general">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:object-identifier"/>
        <xs:element ref="igc:catalogue-identifier"/>
        <xs:element ref="igc:modificator-identifier"/>
        <xs:element ref="igc:responsible-identifier"/>
        <xs:element ref="igc:object-class"/>
        <xs:element ref="igc:title"/>
        <xs:element minOccurs="0" ref="igc:abstract"/>
        <xs:element minOccurs="0" ref="igc:date-of-last-modification"/>
        <xs:element minOccurs="0" ref="igc:date-of-creation"/>
        <xs:element minOccurs="0" ref="igc:original-control-identifier"/>
        <xs:element minOccurs="0" ref="igc:general-additional-values"/>
        <xs:element ref="igc:metadata"/>
        <xs:element minOccurs="0" ref="igc:dataset-alternate-name"/>
        <xs:element minOccurs="0" ref="igc:dataset-character-set"/>
        <xs:element ref="igc:topic-categories"/>
        <xs:element minOccurs="0" ref="igc:env-information"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Objekt Uuid
    Tabelle.Feld   : T01_object.obj_uuid
  -->
  <xs:element name="object-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Verantwortlicher
    Tabelle.Feld   : T01_object.responsible_uuid
    Tabelle.Feld   : t02_address.responsible_uuid
  -->
  <xs:element name="responsible-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Objektklasse
    Tabelle.Feld   : T01_object.obj_class
  -->
  <xs:element name="object-class" type="igc:id"/>
  <!--
    Fachlicher Name: Objektname
    Tabelle.Feld   : T01_object.obj_name
  -->
  <xs:element name="title" type="xs:string"/>
  <!--
    Fachlicher Name: Beschreibung
    Tabelle.Feld   : T01_object.obj_descr
  -->
  <xs:element name="abstract" type="xs:string"/>
  <!--
    Fachlicher Name: Original ID
    Tabelle.Feld   : T01_object.org_obj_id
  -->
  <xs:element name="original-control-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Zusatzfelder
    Tabelle   : t08_attr*
  -->
  <xs:element name="general-additional-values">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:general-additional-value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Zusatzfeld (generische Attribute)
    Tabelle.Feld   : t08_attr_type.id
    Tabelle   : t08_attr_type, t08_attr
  -->
  <xs:element name="general-additional-value">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:field-name"/>
        <xs:element ref="igc:field-value"/>
      </xs:sequence>
      <xs:attributeGroup ref="igc:attlist.igc.id"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Attributname
    Tabelle.Feld   : t08_attr_type.name
  -->
  <xs:element name="field-name" type="xs:string"/>
  <!--
    Fachlicher Name: Attributwert
    Tabelle.Feld   : t08_attr.data
  -->
  <xs:element name="field-value" type="xs:string"/>
  <!-- Neu in T01_object, Elemente von general -->
  <xs:element name="metadata">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="igc:metadata-standard-name"/>
        <xs:element minOccurs="0" ref="igc:metadata-standard-version"/>
        <xs:element minOccurs="0" ref="igc:metadata-character-set"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Name des Metadatenstandards
    Tabelle.Feld   : statisch T01_object.metadata_standard_name
  -->
  <xs:element name="metadata-standard-name" type="xs:string" default="ISO-19115"/>
  <!--
    Fachlicher Name: Version des Metadatenstandards
    Tabelle.Feld   : statisch T01_object.metadata_standard_version
  -->
  <xs:element name="metadata-standard-version" type="xs:string" default="1.0"/>
  <!--
    Fachlicher Name: Zeichensatz des Metadatensatzes
    Tabelle.Feld   : T01_object.metadata_character_set
  -->
  <xs:element name="metadata-character-set" type="igc:iso-code"/>
  <!--
    Fachlicher Name: Kurzbeschreibung
    Tabelle.Feld   : T01_object.dataset_alternate_name
  -->
  <xs:element name="dataset-alternate-name" type="xs:string"/>
  <!--
    Fachlicher Name: Zeichensatz des Datensatzes
    Tabelle.Feld   : T01_object.dataset_character_set
  -->
  <xs:element name="dataset-character-set" type="igc:iso-code"/>
  <!--
    Fachlicher Name: Themenkategorien eines Objektes
    Tabelle.Feld   : t011_obj_topic_cat
  -->
  <xs:element name="topic-categories">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="igc:topic-category"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Themenkategorie eines Objektes
    Tabelle.Feld   : t011_obj_topic_cat.topic_category
  -->
  <xs:element name="topic-category" type="igc:id"/>
  <!--
    Fachlicher Name: Katalog Umweltthemen Infos (Katalogseite)
    Tabellen   : T01_object, T0114_env_topic, T0114_env_category
  -->
  <xs:element name="env-information">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:is-catalog"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:env-topic"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:env-category"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Als Katalogseite in Umweltthemen anzeigen
    Tabelle.Feld   : T01_object.is_catalog_data
  -->
  <xs:element name="is-catalog" default="N" type="igc:igc-boolean"/>
  <!--
    Fachlicher Name: Umweltthema SchlÃ¼ssel (Katalogseite)
    Tabelle.Feld   : T0114_env_topic.topic_key
  -->
  <xs:element name="env-topic" type="igc:id"/>
  <!--
    Fachlicher Name: Umweltkategorie SchlÃ¼ssel (Katalogseite)
    Tabelle.Feld   : t0114_env_category.cat_key
  -->
  <xs:element name="env-category" type="igc:id"/>
  <!--
    Fachlicher Name: Objektklassen
  -->
  <xs:element name="technical-domain">
    <xs:complexType>
      <xs:choice minOccurs="0">
        <xs:element ref="igc:dataset"/>
        <xs:element ref="igc:service"/>
        <xs:element ref="igc:document"/>
        <xs:element ref="igc:map"/>
        <xs:element ref="igc:project"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Datensammlung /Datenbank
    Tabelle   : t011_obj_data*
  -->
  <xs:element name="dataset">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="igc:description-of-tech-domain"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:dataset-parameter"/>
        <xs:element minOccurs="0" ref="igc:method"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Erlã´´erung zum Fachbezug
    Tabelle.Feld   : T011_obj_*.description
  -->
  <xs:element name="description-of-tech-domain" type="xs:string"/>
  <!--
    Fachlicher Name: Inhalte / Ergaenzende Angaben der Datensammlung/Datenbank
    Tabelle   : T011_obj_data_para
  -->
  <xs:element name="dataset-parameter">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:parameter"/>
        <xs:element minOccurs="0" ref="igc:supplemantary-information"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Inhalte der Datensammlung/Datenbank
    Tabelle.Feld   : T011_obj_data_para.parameter
  -->
  <xs:element name="parameter" type="xs:string"/>
  <!--
    Fachlicher Name: Ergaenzende Angaben (Hinweis: Einheit der Parameter)
    Tabelle.Feld   : T011_obj_data_para.unit
  -->
  <xs:element name="supplemantary-information" type="xs:string"/>
  <!--
    Fachlicher Name: Methode/Datengrundlage
    Tabelle.Feld   : T011_obj_data.base
  -->
  <xs:element name="method" type="xs:string"/>
  <!--
    Fachlicher Name: Dienst / Anwendung / Informationssystem
    Tabelle   : t011_obj_serv*
  -->
  <xs:element name="service">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:description-of-tech-domain"/>
        <xs:element ref="igc:service-classification"/>
        <xs:element maxOccurs="unbounded" ref="igc:service-type"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:publication-scale"/>
        <xs:element minOccurs="0" ref="igc:system-history"/>
        <xs:element minOccurs="0" ref="igc:database-of-system"/>
        <xs:element minOccurs="0" ref="igc:system-environment"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:service-version"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:service-operation"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Service-Klassifikation (INSPIRE)
    Tabelle.Feld   : T011_obj_serv.type_key
    Tabelle.Feld   : T011_obj_serv.type_value
  -->
  <xs:element name="service-classification" type="igc:id-string"/>
  <!--
    Fachlicher Name: Service-Typ (INSPIRE Classification of spatial data services)
    Tabelle.Feld   : T011_obj_serv_type.serv_type_key
    Tabelle.Feld   : T011_obj_serv_type.serv_type_value
  -->
  <xs:element name="service-type" type="igc:id-string"/>
  <!--
    Fachlicher Name: Historie
    Tabelle.Feld   : T011_obj_serv.history
  -->
  <xs:element name="system-history" type="xs:string"/>
  <!--
    Fachlicher Name: Basisdaten
    Tabelle.Feld   : T011_obj_serv.base
  -->
  <xs:element name="database-of-system" type="xs:string"/>
  <!--
    Fachlicher Name: Systemumgebung
    Tabelle.Feld   : T011_obj_serv.environment
  -->
  <xs:element name="system-environment" type="xs:string"/>
  <!--
    Fachlicher Name: Version des Service
    Tabelle.Feld   : T011_obj_serv_version.version
  -->
  <xs:element name="service-version" type="xs:string"/>
  <!--
    Fachlicher Name: Operation
    Tabelle   : T011_obj_serv_op*
  -->
  <xs:element name="service-operation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:operation-name"/>
        <xs:element ref="igc:description-of-operation"/>
        <xs:element minOccurs="0" ref="igc:invocation-name"/>
        <xs:element maxOccurs="unbounded" ref="igc:platform"/>
        <xs:element maxOccurs="unbounded" ref="igc:connection-point"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:parameter-of-operation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:depends-on"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Operationbeschreibung
    Tabelle.Feld   : T011_obj_serv_operation.descr
  -->
  <xs:element name="description-of-operation" type="xs:string"/>
  <!--
    Fachlicher Name: Operation-Namensschluessel / -Name
    Tabelle.Feld   : T011_obj_serv_operation.name_key
    Tabelle.Feld   : T011_obj_serv_operation.name_value
  -->
  <xs:element name="operation-name" type="igc:id-string"/>
  <!--
    Fachlicher Name: Aufruf
    Tabelle.Feld   : T011_obj_serv_operation.invocation_name
  -->
  <xs:element name="invocation-name" type="xs:string"/>
  <!--
    Fachlicher Name: Platform
    Tabelle.Feld   : T011_obj_serv_op_platform.platform
  -->
  <xs:element name="platform" type="xs:string"/>
  <!--
    Fachlicher Name: Parameter der Operation
    Tabelle   : T011_obj_serv_op_para
  -->
  <xs:element name="parameter-of-operation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:name"/>
        <xs:element ref="igc:optional"/>
        <xs:element ref="igc:repeatability"/>
        <xs:element ref="igc:direction"/>
        <xs:element minOccurs="0" ref="igc:description-of-parameter"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Attribute des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.optional
  -->
  <xs:element name="optional" type="xs:string"/>
  <!--
    Fachlicher Name: Attribute des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.repeatability
  -->
  <xs:element name="repeatability" type="xs:string"/>
  <!--
    Fachlicher Name: Attribute des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.direction
  -->
  <xs:element name="direction" type="xs:string"/>
  <!--
    Fachlicher Name: Beschreibung des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.descr
  -->
  <xs:element name="description-of-parameter" type="xs:string"/>
  <!--
    Fachlicher Name: AbhÃ¤ngigkeiten
    Tabelle.Feld   : T011_obj_serv_op_depends.depends_on
  -->
  <xs:element name="depends-on" type="xs:string"/>
  <!--
    Fachlicher Name: Zugriffsadresse
    Tabelle.Feld   : t011_obj_serv_op_connpoint.connect_point
  -->
  <xs:element name="connection-point" type="xs:string"/>
  <!--
    Fachlicher Name: Gutachten / Bericht / Literatur
    Tabelle   : t011_obj_literature
  -->
  <xs:element name="document">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="igc:description-of-tech-domain"/>
        <xs:element minOccurs="0" ref="igc:publisher"/>
        <xs:element minOccurs="0" ref="igc:publishing-place"/>
        <xs:element minOccurs="0" ref="igc:year"/>
        <xs:element minOccurs="0" ref="igc:isbn"/>
        <xs:element minOccurs="0" ref="igc:source"/>
        <xs:element minOccurs="0" ref="igc:type-of-document"/>
        <xs:element minOccurs="0" ref="igc:editor"/>
        <xs:element minOccurs="0" ref="igc:author"/>
        <xs:element minOccurs="0" ref="igc:additional-bibliographic-info"/>
        <xs:element minOccurs="0" ref="igc:location"/>
        <xs:element minOccurs="0" ref="igc:pages"/>
        <xs:element minOccurs="0" ref="igc:volume"/>
        <xs:element minOccurs="0" ref="igc:published-in"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Verlag
    Tabelle.Feld   : t011_obj_literature.publishing
  -->
  <xs:element name="publisher" type="xs:string"/>
  <!--
    Fachlicher Name: Erscheinungsort
    Tabelle.Feld   : t011_obj_literature.publish_loc
  -->
  <xs:element name="publishing-place" type="xs:string"/>
  <!--
    Fachlicher Name: Erscheinungsjahr
    Tabelle.Feld   : t011_obj_literature.publish_year
  -->
  <xs:element name="year" type="xs:string"/>
  <!--
    Fachlicher Name: ISBN-Nr.
    Tabelle.Feld   : t011_obj_literature.isbn
  -->
  <xs:element name="isbn" type="xs:string"/>
  <!--
    Fachlicher Name: Basisdaten
    Tabelle.Feld   : t011_obj_literature.base
  -->
  <xs:element name="source" type="xs:string"/>
  <!--
    Fachlicher Name: Dokumententyp
    Tabelle.Feld   : t011_obj_literature.type_key
    Tabelle.Feld   : t011_obj_literature.type_value
  -->
  <xs:element name="type-of-document" type="igc:id-string"/>
  <!--
    Fachlicher Name: Herausgeber
    Tabelle.Feld   : t011_obj_literature.publisher
  -->
  <xs:element name="editor" type="xs:string"/>
  <!--
    Fachlicher Name: Autor/Verfasser
    Tabelle.Feld   : t011_obj_literature.author
  -->
  <xs:element name="author" type="xs:string"/>
  <!--
    Fachlicher Name: Weitere bibliographische Angaben
    Tabelle.Feld   : t011_obj_literature.doc_info
  -->
  <xs:element name="additional-bibliographic-info" type="xs:string"/>
  <!--
    Fachlicher Name: Standort
    Tabelle.Feld   : T011_obj_literatur.loc
  -->
  <xs:element name="location" type="xs:string"/>
  <!--
    Fachlicher Name: Seiten
    Tabelle.Feld   : t011_obj_literature.sides
  -->
  <xs:element name="pages" type="xs:string"/>
  <!--
    Fachlicher Name: Band, Heft
    Tabelle.Feld   : t011_obj_literature.volume
  -->
  <xs:element name="volume" type="xs:string"/>
  <!--
    Fachlicher Name: Erschienen in
    Tabelle.Feld   : t011_obj_literature.publish_in
  -->
  <xs:element name="published-in" type="xs:string"/>
  <!--
    Fachlicher Name: Geo-Information /Karte
    Tabelle   : t011_obj_geo*
  -->
  <xs:element name="map">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="igc:hierarchy-level"/>
        <xs:element minOccurs="0" ref="igc:description-of-tech-domain"/>
        <xs:element minOccurs="0" ref="igc:data"/>
        <xs:element minOccurs="0" ref="igc:resolution"/>
        <xs:element minOccurs="0" ref="igc:coordinate-system"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:publication-scale"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="igc:key-catalogue"/>
        <xs:element minOccurs="0" ref="igc:degree-of-record"/>

    Tabelle.Feld   : T011_obj_geo.special_base
  -->
  <xs:element name="technical-base" type="xs:string"/>
  <xs:element name="symbol-catalogue">
    <xs:complexType>
      <xs:attributeGroup ref="udk:attlist.udk.symbol-catalogue"/>
    </xs:complexType>
  </xs:element>
  <!-- Neu in T011_obj_geo_symc: -->
  <!--
    Fachlicher Name: Symbolkatalog:Titel
    Tabelle.Feld   : T011_obj_geo_symc.symbol_cat
  -->
  <!--
    Fachlicher Name: Symbolkatalog: Datum
    Tabelle.Feld   : T011_obj_geo_symc.symbol_date
  -->
  <!--
    Fachlicher Name: Symbollkatalog: Version
    Tabelle.Feld   : T011_obj_geo_symc.edition
  -->
  <xs:attributeGroup name="attlist.udk.symbol-catalogue">
    <xs:attribute name="symbol-cat"/>

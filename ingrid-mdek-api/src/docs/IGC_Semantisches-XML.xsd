  <!--
    Fachlicher Name: Beschreibung
    Tabelle.Feld   : T01_object.obj_descr
  -->
  <xs:element name="abstract" type="xs:string"/>
  <!--
    Fachlicher Name: ??
    Tabelle.Feld   : immer: "german" / ignoriert bei Eing.
  -->
  <xs:element name="language-of-resource">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="udk:attlist.udk.language-of-resource"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: ??
    Tabelle.Feld   : immer: "german" / ignoriert bei Eing.
  -->
  <xs:element name="language-of-record">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="udk:attlist.udk.language-of-record"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Original ID
    Tabelle.Feld   : T01_object.orig_id
  -->
  <xs:element name="original-control-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Anzahl der Eltern-Objekte
    Tabelle.Feld   : berechnet
  -->
  <xs:element name="no-of-parents" type="xs:string"/>
  <!--
    Fachlicher Name: generische Attribute fuer Zusatzklassen (z.Zt. nicht genutzt)
    Tabelle.Feld   :
  -->
  <xs:element name="general-additional-values">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="udk:field-name"/>
        <xs:element ref="udk:field-value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Attributname
    Tabelle.Feld   :
  -->
  <xs:element name="field-name" type="xs:string"/>
  <!--
    Fachlicher Name: Attributwert
    Tabelle.Feld   :
  -->
  <xs:element name="field-value" type="xs:string"/>
  <!-- Neu in T01_object, Elemente von general -->
  <xs:element name="metadata">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="udk:metadata-character-set"/>
      </xs:sequence>
      <xs:attributeGroup ref="udk:attlist.udk.metadata"/>
    </xs:complexType>
  </xs:element>

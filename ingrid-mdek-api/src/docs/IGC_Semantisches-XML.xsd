    Fachlicher Name:
    Tabelle.Feld   : T01_object.metadata_character_set
  -->
  <xs:attributeGroup name="attlist.udk.metadata-character-set">
    <xs:attribute name="domain-id" use="required"/>
  </xs:attributeGroup>
  <!-- keine fachliche Beschreibung vorhanden -->
  <!--
    Fachlicher Name: Kurzbeschreibung
    Tabelle.Feld   : T01_object.dataset_alternate_name
  -->
  <xs:element name="dataset-alternate-name" type="xs:string"/>
  <!-- keine fachliche Beschreibung vorhanden -->
  <!--
    Fachlicher Name: Zeichensatz des Datensatzes
    Tabelle.Feld   :
  -->
  <xs:element name="dataset-character-set">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="udk:attlist.udk.dataset-character-set"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name:
    Tabelle.Feld   : T01_object.dataset_character_set
  -->
  <xs:attributeGroup name="attlist.udk.dataset-character-set">
    <xs:attribute name="domain-id" use="required"/>
  </xs:attributeGroup>
  <xs:element name="technical-domain">
    <xs:complexType>
      <xs:choice minOccurs="0">
        <xs:element ref="udk:dataset"/>
        <xs:element ref="udk:service"/>
        <xs:element ref="udk:document"/>
        <xs:element ref="udk:map"/>
        <xs:element ref="udk:project"/>
        <xs:element ref="udk:additional-class-attribute-values"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="dataset">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="udk:description-of-tech-domain"/>
        <xs:sequence minOccurs="0" maxOccurs="unbounded">
          <xs:element minOccurs="0" ref="udk:parameter"/>
          <xs:element minOccurs="0" ref="udk:supplemantary-information"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="udk:method"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Erl㴴erung zum Fachbezug
    Tabelle.Feld   : T011_obj_*.description
  -->
  <xs:element name="description-of-tech-domain" type="xs:string"/>
  <!--
    Fachlicher Name: Inhalte der Datensammlung/Datenbank
    Tabelle.Feld   : T011_obj_data_para.parameter
  -->
  <xs:element name="parameter" type="xs:string"/>
  <!--
    Fachlicher Name: Ergaenzende Angaben
    Tabelle.Feld   : T011_obj_dat_para.unit
  -->
  <xs:element name="supplemantary-information" type="xs:string"/>
  <!--
    Fachlicher Name: Methode/Datengrundlage
    Tabelle.Feld   : T011_obj_data.base
  -->
  <xs:element name="method" type="xs:string"/>
  <xs:element name="service">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="udk:description-of-tech-domain"/>
        <xs:element minOccurs="0" ref="udk:system-history"/>
        <xs:element minOccurs="0" ref="udk:database-of-system"/>
        <xs:element minOccurs="0" ref="udk:service-type"/>
        <xs:element minOccurs="0" ref="udk:system-environment"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:service-version"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="udk:service-operation"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Historie
    Tabelle.Feld   : T011_obj_serv.history
  -->
  <xs:element name="system-history" type="xs:string"/>
  <!--
    Fachlicher Name: Basisdaten
    Tabelle.Feld   : T011_obj_serv.base
  -->
  <xs:element name="database-of-system" type="xs:string"/>
  <!--
    Fachlicher Name: Servicetyp
    Tabelle.Feld   : T011_obj_serv.type
  -->
  <xs:element name="service-type" type="xs:string"/>
  <!--
    Fachlicher Name: Systemumgebung
    Tabelle.Feld   : T011_obj_serv.environment
  -->
  <xs:element name="system-environment" type="xs:string"/>
  <!--
    Fachlicher Name: Version des Service
    Tabelle.Feld   : T011_obj_serv_version.version
  -->
  <xs:element name="service-version" type="xs:string"/>
  <xs:element name="service-operation">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="udk:udk.service-operation-description">
          <xs:attributeGroup ref="udk:attlist.udk.service-operation"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Operationname
    Tabelle.Feld   : T011_obj_serv.name
  -->
  <xs:attributeGroup name="attlist.udk.service-operation">
    <xs:attribute name="name" use="required"/>
  </xs:attributeGroup>
  <!--
    Fachlicher Name: Operationbeschreibung
    Tabelle.Feld   : T011_obj_serv.descr
  -->
  <xs:complexType name="udk.service-operation-description">
    <xs:sequence>
      <xs:element ref="udk:service-operation-description"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="service-operation-description" type="xs:string"/>
  <xs:element name="document">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="udk:description-of-tech-domain"/>
        <xs:element minOccurs="0" ref="udk:publisher"/>
        <xs:element minOccurs="0" ref="udk:publishing-place"/>
        <xs:element minOccurs="0" ref="udk:year"/>
        <xs:element minOccurs="0" ref="udk:isbn"/>
        <xs:element minOccurs="0" ref="udk:source"/>
        <xs:element minOccurs="0" ref="udk:type-of-document"/>
        <xs:element minOccurs="0" ref="udk:editor"/>
        <xs:element minOccurs="0" ref="udk:author"/>
        <xs:element minOccurs="0" ref="udk:additional-bibliographic-info"/>
        <xs:element minOccurs="0" ref="udk:location"/>
        <xs:element minOccurs="0" ref="udk:pages"/>
        <xs:element minOccurs="0" ref="udk:volume"/>
        <xs:element minOccurs="0" ref="udk:published-in"/>

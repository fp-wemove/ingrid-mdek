<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:complexType name="MDEK_REQUEST_MAP">
		<xs:all>
			<!-- user-id specifies current user for tracking running jobs -->
			<xs:element name="user-id" type="xs:string"/>
			<xs:element ref="uuid"/>
			<xs:element ref="parent-uuid"/>
			<xs:element name="from-uuid" type="UUID"/>
			<xs:element name="to-uuid" type="UUID"/>
			<!-- request specific sys-lists -->
			<xs:element name="sys-list-ids" type="INTEGER_ARRAY"/>
			<!-- request specific sys-guis -->
			<xs:element name="sys-gui-ids" type="STRING_ARRAY"/>
			<!-- WRITE sysguis -->
			<xs:element name="sys-gui-list" type="SYS_GUI_ELEMENT_LIST"/>
			<!-- request specific sys-additional-fields -->
			<xs:element name="sys-additional-field-ids" type="LONG_ARRAY"/>
			<xs:element ref="language"/>
			<xs:element name="OBJECT_MAP" type="OBJECT_MAP"/>
			<!-- how much objects referencing an address to fetch and where to start (needed for paging of object references in address) -->
			<xs:element name="objReferencesFrom_startIndex" type="xs:integer"/>
			<xs:element name="objReferencesFrom_maxNum" type="xs:integer"/>
			<xs:element name="ADDRESS_MAP" type="ADDRESS_MAP"/>
			<xs:element name="REQUEST_ADDITIONAL_INFO_MAP" type="REQUEST_ADDITIONAL_INFO_MAP"/>
			<xs:element name="REQUEST_SEARCH_MAP" type="REQUEST_SEARCH_MAP"/>
			<xs:element name="REQUEST_SECURITY_MAP" type="REQUEST_SECURITY_MAP"/>
			<xs:element name="REQUEST_EXPORT_MAP" type="REQUEST_EXPORT_MAP"/>
			<xs:element name="REQUEST_IMPORT_MAP" type="REQUEST_IMPORT_MAP"/>
		</xs:all>
	</xs:complexType>

	<xs:complexType name="MDEK_RESULT_MAP">
		<xs:all>
			<xs:element name="OBJECT_MAP" type="OBJECT_MAP"/>
			<xs:element name="ADDRESS_MAP" type="ADDRESS_MAP"/>
			<!-- added here, returned when paging, e.g. when fetching QA entities. Also part of specific description of results, e.g. in RESULT_STATISTICS_MAP -->
			<xs:element name="total-num-paging" type="xs:long"/>
			<!-- total number of entities assigned to QA -->
			<xs:element name="total-num-qa-assigned" type="xs:long"/>
			<!-- total number of entities reassigned from QA -->
			<xs:element name="total-num-qa-reassigned" type="xs:long"/>
			<!-- number of entities in list -->
			<xs:element name="total-num" type="xs:long"/>
			<xs:element name="objEntities" type="OBJECT_LIST"/>
			<xs:element name="adrEntities" type="ADDRESS_LIST"/>
			<xs:element name="path" type="UUID_LIST"/>
			<xs:element name="path_organisations" type="PATH_LIST"/>
			<xs:element name="RESULT_ADDITIONAL_INFO_MAP" type="RESULT_ADDITIONAL_INFO_MAP"/>
			<xs:element name="RESULT_SYS_LIST_MAP" type="RESULT_SYS_LIST_MAP"/>
			<xs:element name="RESULT_SYS_GUI_MAP" type="RESULT_SYS_GUI_MAP"/>
			<xs:element name="RESULT_SYS_ADDITIONAL_FIELDS_MAP" type="RESULT_SYS_ADDITIONAL_FIELDS_MAP"/>
			<xs:element name="RESULT_CATALOG_MAP" type="RESULT_CATALOG_MAP"/>
			<xs:element name="RESULT_RUNNING_JOBS_MAP" type="RESULT_RUNNING_JOBS_MAP"/>
			<xs:element name="RESULT_SEARCH_MAP" type="RESULT_SEARCH_MAP"/>
			<xs:element name="RESULT_VERSION_MAP" type="RESULT_VERSION_MAP"/>
			<xs:element name="RESULT_SECURITY_MAP" type="RESULT_SECURITY_MAP"/>
			<xs:element name="RESULT_STATISTICS_MAP" type="RESULT_STATISTICS_MAP"/>
			<xs:element name="RESULT_EXPORT_MAP" type="RESULT_EXPORT_MAP"/>
			<xs:element name="RESULT_IMPORT_MAP" type="RESULT_IMPORT_MAP"/>
		</xs:all>
	</xs:complexType>

	<!-- Additional Error Info when Mdek Exception is thrown concerning Security -->
	<xs:complexType name="EXCEPTION_SECURITY_INFO_MAP">
		<xs:sequence>
			<xs:choice>
				<!-- IDCUsers ! e.g. IdcUsers in group when group deleted and no forceDeleteGroupWhenUsers ... -->
				<xs:element name="idc-users" type="IDC_USER_LIST"/>
				<!-- e.g. mod-user or responsible-user who has no permission anymore -->
				<xs:element name="user-addresses" type="ADDRESS_LIST"/>
			</xs:choice>
			<xs:choice>
				<!-- The Object(s) affected by the exception. NOTICE: When multiple then order is important (parent/child) -->
				<xs:element name="objEntities" type="OBJECT_LIST"/>
				<!-- The Address(es) affected by the exception. NOTICE: When multiple then order is important (parent/child) -->
				<xs:element name="adrEntities" type="ADDRESS_LIST"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="OBJECT_MAP">
		<xs:all>
			<xs:element ref="id"/>
			<xs:element ref="entity-metadata-id"/>
			<xs:element ref="parent-uuid"/>
			<xs:element ref="uuid"/>
			<xs:element ref="class"/>
			<xs:element ref="hasChild"/>
			<xs:element ref="isPublished"/>
			<xs:element ref="date-of-creation"/>
			<xs:element ref="date-of-last-modification"/>
			<xs:element ref="work-state"/>
			<xs:element name="dataset-alternate-name" type="xs:string"/>
			<xs:element name="title" type="xs:string"/>
			<xs:element name="abstract" type="xs:string"/>
			
			<xs:element name="SPATIAL_REF_MAP" type="SPATIAL_REF_MAP"/>
			<xs:element name="TIME_MAP" type="TIME_MAP"/>
			
			<xs:element name="metadata-language" type="xs:string"/>
			<xs:element name="data-language" type="xs:string"/>
			<xs:element name="publication-condition" type="xs:integer"/>
			<xs:element name="dataset-intentions" type="xs:string"/>
			<xs:element name="dataset-usage" type="xs:string"/>
			
			<xs:element name="ordering-instructions" type="xs:string"/>
			<xs:element name="is-catalog-data" type="xs:string"/>
			
			<xs:element name="original-control-identifier" type="xs:string"/>
			<xs:element name="catalogue-identifier" type="xs:long"/>
			<xs:element name="dataset-character-set" type="xs:integer"/>
			<xs:element name="metadata-character-set" type="xs:integer"/>
			<xs:element name="metadata-standard-name" type="xs:string"/>
			<xs:element name="metadata-standard-version" type="xs:string"/>
			<xs:element name="lastexport-time" type="TIMESTAMP"/>
			<xs:element name="expiry-state" type="xs:integer"/>
			<xs:element name="mark-deleted" type="xs:string"/>
			<xs:element ref="mod-user"/>
			<xs:element ref="responsible-user"/>
			<xs:element name="assigner-uuid" type="xs:string"/>
			<xs:element name="assigner-user"  type="ADDRESS_MAP"/>
			<xs:element name="assign-time" type="TIMESTAMP"/>
			<xs:element name="reassigner-uuid" type="xs:string"/>
			<xs:element name="reassign-time" type="TIMESTAMP"/>
			
			<xs:element name="linkages" type="LINKAGE_LIST"/>
			<xs:element name="dataset-references" type="DATASET_REFERENCE_LIST"/>
			<xs:element name="export-criteria" type="EXPORT_CRITERIA_LIST"/>
			<xs:element name="legislations" type="LEGISLATION_LIST"/>
			<xs:element name="data-formats" type="DATA_FORMAT_LIST"/>
			<xs:element name="medium-options" type="MEDIUM_OPTION_LIST"/>
			<xs:element name="subject-terms" type="SUBJECT_TERM_LIST"/>
			<xs:element name="env-categories" type="INTEGER_LIST"/>
			<xs:element name="env-topics" type="INTEGER_LIST"/>
			<xs:element name="topic-categories" type="INTEGER_LIST"/>
			<xs:element name="comment_list" type="COMMENT_LIST"/>

			<xs:element name="OBJ_RELATIONS_MAP" type="OBJ_RELATIONS_MAP"/>

			<xs:element name="technical-domain-map" type="TECHNICAL_DOMAIN_MAP_MAP"/>
			<xs:element name="technical-domain-service" type="TECHNICAL_DOMAIN_SERVICE_MAP"/>
			<xs:element name="technical-domain-project" type="TECHNICAL_DOMAIN_PROJECT_MAP"/>
			<xs:element name="technical-domain-dataset" type="TECHNICAL_DOMAIN_DATASET_MAP"/>
			<xs:element name="technical-domain-document" type="TECHNICAL_DOMAIN_DOCUMENT_MAP"/>
			<xs:element name="parent-info" type="OBJ_PARENT_INFO_MAP"/>

			<!-- holds permissions of user on entity -->
			<xs:element name="idc-permissions" type="PERMISSION_LIST"/>
			
			<xs:element name="additional-fields" type="ADDITIONAL_FIELD_LIST"/>

			<!-- INSPIRE -->
			<xs:element name="conformity_list" type="CONFORMITY_LIST"/>
			<xs:element name="access_list" type="ACCESS_LIST"/>
			
		</xs:all>
	</xs:complexType>
	<xs:complexType name="ADDRESS_MAP">
		<xs:all>
			<xs:element ref="id"/>
			<xs:element ref="entity-metadata-id"/>
			<xs:element ref="parent-uuid"/>
			<xs:element ref="uuid"/>
			<xs:element ref="class"/>
			<xs:element ref="hasChild"/>
			<xs:element ref="date-of-creation"/>
			<xs:element ref="date-of-last-modification"/>
			<xs:element ref="work-state"/>
			<xs:element ref="organisation"/>
			<xs:element ref="name"/>
			<xs:element ref="given-name"/>
			<xs:element name="name-form" type="xs:string"/>
			<xs:element name="name-form-key" type="xs:integer"/>
			<xs:element name="title-or-function" type="xs:string"/>
			<xs:element name="title-or-function-key" type="xs:integer"/>
			<xs:element name="street" type="xs:string"/>
			<xs:element name="postal-code-of-country" type="xs:string"/>
			<xs:element name="postal-code" type="xs:string"/>
			<xs:element name="city" type="xs:string"/>
			<xs:element name="post-box-postal-code" type="xs:string"/>
			<xs:element name="post-box" type="xs:string"/>
			<xs:element name="function" type="xs:string"/>
			<xs:element name="address-description" type="xs:string"/>
			<xs:element name="original-address-identifier" type="xs:string"/>

			<xs:element name="lastexport-time" type="TIMESTAMP"/>
			<xs:element name="expiry-state" type="xs:integer"/>
			<xs:element name="mark-deleted" type="xs:string"/>
			<xs:element ref="mod-user"/>
			<xs:element ref="responsible-user"/>
			<xs:element name="assigner-uuid" type="xs:string"/>
			<xs:element name="assign-time" type="TIMESTAMP"/>
			<xs:element name="reassigner-uuid" type="xs:string"/>
			<xs:element name="reassign-time" type="TIMESTAMP"/>
			
			<xs:element name="communication" type="COMMUNICATION_LIST"/>
			<xs:element name="subject-terms" type="SUBJECT_TERM_LIST"/>
			<xs:element name="comment_list" type="COMMENT_LIST"/>
			
			<xs:element name="ADR_RELATIONS_MAP" type="ADR_RELATIONS_MAP"/>
			<xs:element name="parent-info" type="ADDR_PARENT_INFO_MAP"/>

			<!-- holds permissions of user on entity -->
			<xs:element name="idc-permissions" type="PERMISSION_LIST"/>
		</xs:all>
	</xs:complexType>

	<xs:element name="id" type="xs:long"/>
	<xs:element name="entity-metadata-id" type="xs:long"/>
	<xs:element name="parent-uuid" type="UUID"/>
	<xs:element name="uuid" type="UUID"/>
	<xs:element name="class" type="xs:integer"/>
	<xs:element name="hasChild" type="xs:boolean"/>
	<xs:element name="isPublished" type="xs:boolean"/>
	<xs:element name="date-of-creation" type="TIMESTAMP"/>
	<xs:element name="date-of-last-modification" type="TIMESTAMP"/>
	<xs:element name="work-state" type="xs:string"/>
	<!-- db: special_name -->
	<xs:element name="relation-type-name" type="xs:string"/>
	<!-- db: special_ref  -->
	<xs:element name="relation-type-ref" type="xs:integer"/>
	<xs:element name="mod-user"  type="ADDRESS_MAP"/>
	<xs:element name="responsible-user"  type="ADDRESS_MAP"/>
	<xs:element name="organisation" type="xs:string"/>
	<xs:element name="name" type="xs:string"/>
	<xs:element name="given-name" type="xs:string"/>
	<xs:element name="language" type="xs:string"/>
	<xs:simpleType name="TIMESTAMP">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="UUID">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	
	<xs:complexType name="REQUEST_ADDITIONAL_INFO_MAP">
		<xs:all>
			<xs:element name="requestinfo_refetchEntity" type="xs:boolean"/>
			<xs:element name="requestinfo_copySubtree" type="xs:boolean"/>
			<xs:element name="requestinfo_performCheck" type="xs:boolean"/>
			<xs:element name="requestinfo_forcePublicationCondition" type="xs:boolean"/>
			<xs:element name="requestinfo_onlyFreeAddresses" type="xs:boolean"/>
			<xs:element name="requestinfo_targetIsFreeAddress" type="xs:boolean"/>
			<xs:element name="requestinfo_forceDeleteReferences" type="xs:boolean"/>
			<!-- e.g. update working- or published-version or both, pass MdekUtils.IdcEntityVersion Enumeration const -->
			<xs:element name="requestinfo_whichEntityVersion" type="xs:integer"/>
			<!-- e.g. fetch entity in this work state, pass MdekUtils.WorkState Enumeration const -->
			<xs:element name="requestinfo_whichWorkState" type="xs:integer"/>
			<!-- e.g. what to fetch from backend ? pass IMdekCaller.FetchQuantity Enumeration const -->
			<xs:element name="requestinfo_fetchQuantity" type="xs:integer"/>
			<!-- Which entities to fetch (e.g. expired ones ...), pass MdekUtils.Idc*SelectionType Enumeration const -->
			<xs:element name="requestinfo_entitySelectionType" type="xs:integer"/>
			<!-- How to order fetched entities ? pass MdekUtils.IdcEntityOrderBy Enumeration const -->
			<xs:element name="requestinfo_entityOrderBy" type="xs:integer"/>
			<!-- Order ascending = true ! else descending -->
			<xs:element name="requestinfo_entityOrderAsc" type="xs:boolean"/>
			<!-- e.g. when fetching permissions for entity include check of workflow state (true) or not (false). e.g. in state "Q" no write if not "QA" -->
			<xs:element name="requestinfo_checkWorkflow" type="xs:boolean"/>
			<!-- generic paging parameters ! added for STATISTICS. Notice: we also have separate paging params for search and Object-Address references :( -->
			<xs:element name="requestinfo_startHit" type="xs:integer"/>
			<xs:element name="requestinfo_numHits" type="xs:integer"/>
		</xs:all>
	</xs:complexType>	
	<xs:complexType name="REQUEST_SEARCH_MAP">
		<xs:all>
			<xs:element name="search-start-hit" type="xs:integer"/>
			<!-- number of hits requested starting at start hit -->
			<xs:element name="total-num" type="xs:long"/>
			<xs:element name="search-params" type="SEARCH_PARAMS_MAP"/>
			<xs:element name="search-ext-params" type="SEARCH_EXT_PARAMS_MAP"/>
			<!-- query: HQL query -->
			<xs:element name="hql-query" type="xs:string"/>
		</xs:all>
	</xs:complexType>	
	<xs:complexType name="SEARCH_PARAMS_MAP">
		<xs:choice>
			<!-- address: search -->
			<xs:sequence>
				<xs:element ref="organisation" />
				<xs:element ref="name" />
				<xs:element ref="given-name" />
			</xs:sequence>
			<!-- query: full text -->
			<xs:element name="search_term" type="xs:string" />
			<!-- query: thesaurus term -->
			<xs:element ref="term-sns-id" />
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="SEARCH_EXT_PARAMS_MAP">
		<xs:choice>
			<!-- address -->
			<xs:sequence>
				<!-- address: extended search: subject: search terms  -->
				<xs:element name="query-term" type="xs:string"/>
				<xs:element name="relation" type="xs:integer"/>
				<!-- address: extended search: subject: search mode ('whole word'(0), 'partial word'(1)) -->
				<xs:element name="search-type" type="xs:integer"/>
				<!-- address: extended search: subject: search range ('all text fields'(0), 'institutions, person, description, keywords'(1)) -->
				<xs:element name="search-range" type="xs:integer"/>
				<!-- address: extended search: spatial  -->
				<xs:element name="street" type="xs:string"/>
				<xs:element name="postal-code" type="xs:string"/>
				<xs:element name="city" type="xs:string"/>
			</xs:sequence>
			<!-- object -->
			<xs:sequence>
				<!-- object: extended search: subject: search term relations (and = 0, or = 1)  -->
				<xs:element name="relation" type="xs:integer"/>
				<!-- object: extended search: subject: search terms  -->
				<xs:element name="query-term" type="xs:string"/>
				<!-- address: extended search: subject: search mode ('whole word'(0), 'partial word'(1)) -->
				<xs:element name="search-type" type="xs:integer"/>
				<!-- object: extended search: subject: object classes  -->
				<xs:element name="obj-classes" type="OBJECT_CLASS_LIST"/>
				<!-- object: extended search: subject: dictionary (only the topic_id will be supplied) -->
				<xs:element name="thesaurus-terms" type="SUBJECT_TERM_LIST"/>
				<!-- object: extended search: subject: relations between entries (and = 0, or = 1) -->
				<xs:element name="thesaurus-relation" type="xs:integer"/>
				<!-- object: extended search: spatial: geo thesaurus (only the location-sns-id will be supplied) -->
				<xs:element name="geo-thesaurus-terms" type="LOCATION_LIST"/>
				<!-- object: extended search: spatial: relations between entries (and = 0, or = 1) -->
				<xs:element name="geo-thesaurus-relation" type="xs:integer"/>
				<!-- object: extended search: spatial: free reference  -->
				<xs:element name="custom-location" type="xs:integer"/>
				<!-- object: extended search: time  -->
				<xs:choice>
					<xs:sequence>
						<xs:element name="time-from" type="xs:string"/>
						<xs:element name="time-to" type="xs:string"/>
					</xs:sequence>
					<xs:element name="time-at" type="xs:string"/>
				</xs:choice>
				<xs:element name="time-intersect" type="xs:boolean"/>
				<xs:element name="time-contains" type="xs:boolean"/>
			</xs:sequence>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="REQUEST_SECURITY_MAP">
		<xs:choice>
			<!-- get permissions on Object/Address via uuid -->
			<xs:element ref="uuid"/>
			<!-- get/save group -->
			<xs:element name="GROUP_MAP" type="GROUP_MAP"/>
			<!-- delete group, even when group has users -->
			<xs:element name="requestinfo_forceDeleteGroupWhenUsers" type="xs:boolean"/>
			<!-- also include group of Catalog Admin, e.g. when fetching all groups-->
			<xs:element name="requestinfo_includeCatAdminGroup" type="xs:boolean"/>
			<!-- get/save user -->
			<xs:element name="IDC_USER_MAP" type="IDC_USER_MAP"/>
			<!-- e.g. get detailed permissions of users having write access on entity, then user holds permission list-->
			<xs:element name="requestinfo_getDetailedPermissions" type="xs:boolean"/>
		</xs:choice>
	</xs:complexType>	
	<xs:complexType name="REQUEST_EXPORT_MAP">
		<xs:choice>
			<!-- root of branch to export -->
			<xs:element ref="uuid"/>
			<!-- export only root, no subnodes ? -->
			<xs:element name="requestinfo_exportOnlyRoot" type="xs:boolean"/>
			<!-- which addresses to export if virtual top node (no uuid) is selected, = IMdekCaller.AddressArea Enumeration const -->
			<xs:element name="requestinfo_exportAddressArea" type="xs:integer"/>
			<!-- export only objects marked with given criterion -->
			<xs:element name="export-criterion-value" type="xs:string"/>
			<!-- include export result data in requested export info ? -->
			<xs:element name="requestinfo_exportInfoIncludeData" type="xs:boolean"/>
		</xs:choice>
	</xs:complexType>	
	<xs:complexType name="REQUEST_IMPORT_MAP">
		<xs:choice>
			<xs:element name="requestinfo_importData" type="BYTE_ARRAY"/>
			<!-- object / address node where new objects / addresses are imported underneath -->
			<xs:element name="requestinfo_importObjParentUuid" type="xs:string"/>
			<xs:element name="requestinfo_importAddrParentUuid" type="xs:string"/>
			<!-- publish imported entities immediately ? -->
			<xs:element name="requestinfo_importPublishImmediately" type="xs:boolean"/>
		</xs:choice>
	</xs:complexType>	
	<xs:complexType name="PATH_LIST">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="PATH_MAP" type="PATH_MAP"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PATH_MAP">
		<xs:all>
			<xs:element ref="organisation"/>
			<xs:element ref="class"/>
		</xs:all>
	</xs:complexType>	
	<xs:complexType name="RESULT_ADDITIONAL_INFO_MAP">
		<xs:all>
			<xs:element name="resultinfo_wasFullyDeleted" type="xs:boolean"/>
			<xs:element name="resultinfo_wasMarkedDeleted" type="xs:boolean"/>
			<xs:element name="resultinfo_hasWorkingCopy" type="xs:boolean"/>
			<xs:element name="resultinfo_numberOfProcessedEntities" type="xs:integer"/>
			<xs:element name="resultinfo_uuidOfFoundEntity" type="UUID"/>
			<!-- e.g. type of operation the user performed on an entity, = MdekUtils.UserOperation Enumeration const -->
			<xs:element name="resultinfo_userOperation" type="xs:integer"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="RESULT_SEARCH_MAP">
		<xs:all>
			<!-- number of entities in list -->
			<xs:element name="total-num" type="xs:long"/>
			<xs:element name="adrEntities" type="ADDRESS_LIST"/>
			<xs:element name="objEntities" type="OBJECT_LIST"/>
			<!-- total number of entities for paging -->
			<xs:element name="total-num-paging" type="xs:long"/>
			<!-- csv result as string containing csv lines -->
			<xs:element name="csv-result" type="xs:string"/>
		</xs:all>
	</xs:complexType>	
	<xs:complexType name="RESULT_VERSION_MAP">
		<xs:all>
			<!-- version of mdek api (client )-->
			<xs:element name="api.build.name" type="xs:string"/>
			<xs:element name="api.build.version" type="xs:string"/>
			<xs:element name="api.build.number" type="xs:string"/>
			<xs:element name="api.build.timestamp" type="xs:string"/>
			<!-- version of mdek server (iPlug )-->
			<xs:element name="server.build.name" type="xs:string"/>
			<xs:element name="server.build.version" type="xs:string"/>
			<xs:element name="server.build.number" type="xs:string"/>
			<xs:element name="server.build.timestamp" type="xs:string"/>
		</xs:all>
	</xs:complexType>	
	<xs:complexType name="RESULT_SECURITY_MAP">
		<xs:choice>
			<!-- get group -->
			<xs:element name="GROUP_MAP" type="GROUP_MAP"/>
			<!-- get user -->
			<xs:element name="IDC_USER_MAP" type="IDC_USER_MAP"/>
			<!-- get groups -->
			<xs:element name="groups" type="GROUP_LIST"/>
			<!-- get users -->
			<xs:element name="idc-users" type="IDC_USER_LIST"/>
			<!-- get permissions on object/address -->
			<xs:element name="idc-permissions" type="PERMISSION_LIST"/>
		</xs:choice>
	</xs:complexType>	
	<xs:complexType name="GROUP_LIST">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="GROUP_MAP" type="GROUP_MAP"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GROUP_MAP">
		<xs:all>
			<xs:element name="idc-group-id" type="xs:long"/>
			<xs:element ref="name"/>
			<xs:element ref="date-of-creation"/>
			<xs:element ref="date-of-last-modification"/>
			<xs:element ref="mod-user"/>
			<xs:element name="idc-address-permissions" type="ENTITY_PERMISSION_LIST"/>
			<xs:element name="idc-object-permissions" type="ENTITY_PERMISSION_LIST"/>
			<xs:element name="idc-user-permissions" type="PERMISSION_LIST"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="RESULT_STATISTICS_MAP">
		<xs:choice>
			<!-- STATISTICS_CLASSES_AND_STATES  -->
			<!-- e.g. use as key 0 ("Organisationseinheit") or 2 ("Dokument") and get map with statistics of that entity class (total num and num workstates) -->
			<xs:element name="KEY_IS_ENTITYCLASS_DB_VALUE" type="STATISTICS_ENTITYCLASS_MAP"/>

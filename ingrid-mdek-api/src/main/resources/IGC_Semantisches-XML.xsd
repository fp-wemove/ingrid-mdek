<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.portalu.de/igc-import" xmlns="http://www.portalu.de/igc-import">
    
    <!-- VERSION DES AUSTAUSCHFORMATES -->
  <xs:attributeGroup name="attlist.igc">
    <xs:attribute name="exchange-format" default="4.0.0"/>
  </xs:attributeGroup>
  <!-- InGridCatalog (IGC) containing UDK data -->
  <xs:element name="igc">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="catalogue"/>
        <xs:element minOccurs="0" ref="data-sources"/>
        <xs:element minOccurs="0" ref="addresses"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.igc"/>
    </xs:complexType>
  </xs:element>
  <!-- Katalog Daten -->
  <xs:element name="catalogue">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="catalogue-identifier"/>
        <xs:element ref="catalogue-name"/>
        <xs:element ref="catalogue-namespace"/>
        <xs:element minOccurs="0" ref="partner-name"/>
        <xs:element minOccurs="0" ref="provider-name"/>
        <xs:element minOccurs="0" ref="country"/>
        <xs:element minOccurs="0" ref="language"/>
        <xs:element minOccurs="0" ref="geo-location"/>
        <xs:element minOccurs="0" ref="workflow-control"/>
        <xs:element minOccurs="0" ref="expiry-duration"/>
        <xs:element minOccurs="0" ref="date-of-creation"/>
        <xs:element minOccurs="0" ref="modificator-identifier"/>
        <xs:element minOccurs="0" ref="date-of-last-modification"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Katalog Uuid
    Tabelle.Feld   : T03_catalogue.cat_uuid
    Tabelle.Feld   : T01_object.cat_id
  -->
  <xs:element name="catalogue-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Zuletzt geaendert von
    Tabelle.Feld   : t03_catalogue.mod_uuid
    Tabelle.Feld   : t01_object.mod_uuid
    Tabelle.Feld   : t02_address.mod_uuid
  -->
  <xs:element name="modificator-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Katalog Name
    Tabelle.Feld   : T03_catalogue.cat_name 
  -->
  <xs:element name="catalogue-name" type="xs:string"/>
  <!--
    Fachlicher Name: Katalog Namespace
    Tabelle.Feld   : T03_catalogue.cat_namespace 
  -->
  <xs:element name="catalogue-namespace" type="xs:string"/>
  <!--
    Fachlicher Name: Partner Name
    Tabelle.Feld   : T03_catalogue.partner_name
  -->
  <xs:element name="partner-name" type="xs:string"/>
  <!--
    Fachlicher Name: Anbieter Name
    Tabelle.Feld   : T03_catalogue.provider_name
  -->
  <xs:element name="provider-name" type="xs:string"/>
  <!--
    Fachlicher Name: Land
    Tabelle.Feld   : T03_catalogue.country_key
    Tabelle.Feld   : T03_catalogue.country_value
    Tabelle.Feld   : T02_address.country_key
    Tabelle.Feld   : T02_address.country_value
  -->
  <xs:element name="country" type="id-string"/>
  <!--
    Fachlicher Name: Sprache
    Tabelle.Feld   : T03_catalogue.language_key
    Tabelle.Feld   : T03_catalogue.language_value
  -->
  <xs:element name="language" type="id-string"/>
  <!--
    Fachlicher Name: Workflow Kontrolle
    Tabelle.Feld   : T03_catalogue.workflow_control
  -->
  <xs:element name="workflow-control" type="igc-boolean"/>
  <!--
    Fachlicher Name: Ablaufdauer
    Tabelle.Feld   : T03_catalogue.expiry_duration
  -->
  <xs:element name="expiry-duration" type="xs:string"/>
  <!--
    Fachlicher Name: Zuletzt geaendert am
    Tabelle.Feld   : T03_catalogue.mod_time
    Tabelle.Feld   : T01_object.mod_time
    ...
  -->
  <xs:element name="date-of-last-modification" type="xs:string"/>
  <!--
    Fachlicher Name: Erstellt am
    Tabelle.Feld   : T01_object.create_time
  -->
  <xs:element name="date-of-creation" type="xs:string"/>
  <!--
    Fachlicher Name: Objekt Metadatensätze 
  -->
  <xs:element name="data-sources">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="data-source"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Objekt Metadatensatz
  -->
  <xs:element name="data-source">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="2" ref="data-source-instance"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Objekt Metadatensatz Instanz ("in Bearbeitung" oder "veröffentlicht")
  -->
  <xs:element name="data-source-instance">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="general"/>
        <!-- technical-domain in Klasse 0: Organisationseinheit/Fachaufgabe nicht  vorhanden ->  minOccurs=0 -->
        <xs:element minOccurs="0" ref="technical-domain"/>
        <xs:element ref="additional-information"/>
        <xs:element ref="spatial-domain"/>
        <xs:element minOccurs="0" ref="temporal-domain"/>
        <xs:element minOccurs="0" ref="subject-terms"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="available-linkage"/>
        <xs:element minOccurs="0" ref="parent-data-source"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="related-address"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="link-data-source"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.work-state"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="general">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="object-identifier"/>
        <xs:element ref="catalogue-identifier"/>
        <xs:element ref="modificator-identifier"/>
        <xs:element ref="responsible-identifier"/>
        <xs:element ref="object-class"/>
        <xs:element ref="title"/>
        <xs:element minOccurs="0" ref="abstract"/>
        <xs:element minOccurs="0" ref="date-of-last-modification"/>
        <xs:element minOccurs="0" ref="date-of-creation"/>
        <xs:element minOccurs="0" ref="original-control-identifier"/>
        <xs:element minOccurs="0" ref="general-additional-values"/>
        <xs:element minOccurs="0" ref="metadata"/>
        <xs:element minOccurs="0" ref="dataset-alternate-name"/>
        <xs:element minOccurs="0" ref="dataset-character-set"/>
        <xs:element ref="topic-categories"/>
        <xs:element minOccurs="0" ref="env-information"/>
        <xs:element minOccurs="0" ref="is-inspire-relevant"/>
        <xs:element minOccurs="0" ref="is-open-data"/>
        <xs:element ref="open-data-categories"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Objekt Uuid
    Tabelle.Feld   : T01_object.obj_uuid
  -->
  <xs:element name="object-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Verantwortlicher
    Tabelle.Feld   : T01_object.responsible_uuid
    Tabelle.Feld   : t02_address.responsible_uuid
  -->
  <xs:element name="responsible-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Objektklasse
    Tabelle.Feld   : T01_object.obj_class
  -->
  <xs:element name="object-class" type="id"/>
  <!--
    Fachlicher Name: Objektname
    Tabelle.Feld   : T01_object.obj_name
  -->
  <xs:element name="title" type="xs:string"/>
  <!--
    Fachlicher Name: Beschreibung
    Tabelle.Feld   : T01_object.obj_descr
  -->
  <xs:element name="abstract" type="xs:string"/>
  <!--
    Fachlicher Name: Original ID
    Tabelle.Feld   : T01_object.org_obj_id
  -->
  <xs:element name="original-control-identifier" type="xs:string"/>
  <!--
    Fachlicher Name: Zusatzfelder
    Tabelle   :additional_field_data*
  -->
  <xs:element name="general-additional-values">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="general-additional-value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Zusatzfeld Inhalte
    Tabelle.Feld   : additional_field_data.sort
    Tabelle.Feld   : additional_field_data.field_key
    Tabelle.Feld   : additional_field_data.list_item_id
    Tabelle.Feld   : additional_field_data.data
    Tabelle.Feld   : additional_field_data.parent_field_id (als parent_field_key aufgelöst !)
  -->
  <xs:element name="general-additional-value">
    <xs:complexType>
        <xs:sequence>
        <xs:element ref="field-key"/>
        <xs:element ref="field-data"/>
        <xs:element ref="field-key-parent"/>
        </xs:sequence>
      <xs:attributeGroup ref="attlist.line"/>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Zusatzfeld Identifikation (Key)
    Tabelle.Feld   : additional_field_data.field_key
  -->
  <xs:element name="field-key" type="xs:string"/>
  <!--
    Fachlicher Name: Zusatzfeld Wert (auch Id von Listeneintrag, wenn Liste)
    Tabelle.Feld   : additional_field_data.list_item_id
    Tabelle.Feld   : additional_field_data.data
  -->
  <xs:element name="field-data" type="id-string"/>
  <!--
    Fachlicher Name: Zusatzfeld Identifikation zugehoerige Tabelle (Key)
    Tabelle.Feld   : additional_field_data.parent_field_id (als parent_field_key aufgelöst !)
  -->
  <xs:element name="field-key-parent" type="xs:string"/>
  <!-- Neu in T01_object, Elemente von general -->
  <xs:element name="metadata">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="metadata-standard-name"/>
        <xs:element minOccurs="0" ref="metadata-standard-version"/>
        <xs:element minOccurs="0" ref="metadata-character-set"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Name des Metadatenstandards
    Tabelle.Feld   : statisch T01_object.metadata_standard_name
  -->
  <xs:element name="metadata-standard-name" type="xs:string" default="ISO-19115"/>
  <!--
    Fachlicher Name: Version des Metadatenstandards
    Tabelle.Feld   : statisch T01_object.metadata_standard_version
  -->
  <xs:element name="metadata-standard-version" type="xs:string" default="1.0"/>
  <!--
    Fachlicher Name: Zeichensatz des Metadatensatzes
    Aus IGE Frontend entfernt, s. INGRID-2340
    Tabelle.Feld   : T01_object.metadata_character_set
  -->
  <xs:element name="metadata-character-set" type="iso-code"/>
  <!--
    Fachlicher Name: Kurzbeschreibung
    Tabelle.Feld   : T01_object.dataset_alternate_name
  -->
  <xs:element name="dataset-alternate-name" type="xs:string"/>
  <!--
    Fachlicher Name: Zeichensatz des Datensatzes
    Tabelle.Feld   : T01_object.dataset_character_set
  -->
  <xs:element name="dataset-character-set" type="iso-code"/>
  <!--
    Fachlicher Name: Themenkategorien eines Objektes
    Tabelle.Feld   : t011_obj_topic_cat
  -->
  <xs:element name="topic-categories">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="topic-category"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Themenkategorie eines Objektes
    Tabelle.Feld   : t011_obj_topic_cat.topic_category
  -->
  <xs:element name="topic-category" type="id"/>
  <!--
    Fachlicher Name: Katalog Umweltthemen Infos (Katalogseite)
    Tabellen   : T01_object, T0114_env_topic
  -->
  <xs:element name="env-information">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="is-catalog"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="env-topic"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Als Katalogseite in Umweltthemen anzeigen
    Tabelle.Feld   : T01_object.is_catalog_data
  -->
  <xs:element name="is-catalog" default="N" type="igc-boolean"/>
  <!--
    Fachlicher Name: "INSPIRE-relevanter Datensatz" (checkbox)
    Tabelle.Feld   : T01_object.is_inspire_relevant
  -->
  <xs:element name="is-inspire-relevant" default="N" type="igc-boolean"/>
  <!--
    Fachlicher Name: "Open Data Datensatz" (checkbox)
    Tabelle.Feld   : T01_object.is_open_data
  -->
  <xs:element name="is-open-data" default="N" type="igc-boolean"/>
  <!--
    Fachlicher Name: Open Data Kategorien eines Objektes, see REDMINE-128
    Tabelle.Feld   : object_open_data_category
  -->
  <xs:element name="open-data-categories">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="open-data-category"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Open Data Kategorie eines Objektes, see REDMINE-128
    Tabelle.Feld   : object_open_data_category.category_key
    Tabelle.Feld   : object_open_data_category.category_value
  -->
  <xs:element name="open-data-category" type="id-string"/>
  <!--
    Fachlicher Name: Umweltthema Schlüssel (Katalogseite)
    Tabelle.Feld   : T0114_env_topic.topic_key
  -->
  <xs:element name="env-topic" type="id"/>
  <!--
    Fachlicher Name: Objektklassen
  -->
  <xs:element name="technical-domain">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="dataset"/>
        <xs:element ref="service"/>
        <xs:element ref="document"/>
        <xs:element ref="map"/>
        <xs:element ref="project"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Datensammlung /Datenbank
    Tabelle   : t011_obj_data*
    Tabelle   : object_types_catalogue für key-catalogue
  -->
  <xs:element name="dataset">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="description-of-tech-domain"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="key-catalogue"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="dataset-parameter"/>
        <xs:element minOccurs="0" ref="method"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Erl㴴erung zum Fachbezug
    Tabelle.Feld   : T011_obj_*.description
  -->
  <xs:element name="description-of-tech-domain" type="xs:string"/>
  <!--
    Fachlicher Name: Inhalte / Ergaenzende Angaben der Datensammlung/Datenbank
    Tabelle   : T011_obj_data_para
  -->
  <xs:element name="dataset-parameter">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="parameter"/>
        <xs:element minOccurs="0" ref="supplementary-information"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Inhalte der Datensammlung/Datenbank
    Tabelle.Feld   : T011_obj_data_para.parameter
  -->
  <xs:element name="parameter" type="xs:string"/>
  <!--
    Fachlicher Name: Ergaenzende Angaben (Hinweis: Einheit der Parameter)
    Tabelle.Feld   : T011_obj_data_para.unit
  -->
  <xs:element name="supplementary-information" type="xs:string"/>
  <!--
    Fachlicher Name: Methode/Datengrundlage
    Tabelle.Feld   : T011_obj_data.base
  -->
  <xs:element name="method" type="xs:string"/>
  <!--
    Fachlicher Name: Fachbezug "Geodatendienst" UND "Informationssystem/Dienst/Anwendung" (vormals Klasse "Dienst / Anwendung / Informationssystem", die in InGrid 2.3 geteilt wurde !) 
    Tabelle   : t011_obj_serv*
  -->
  <xs:element name="service">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="description-of-tech-domain"/>
        <xs:element ref="service-type"/>
        <xs:element ref="coupling-type"/>
        <!-- minOccurs=0 da nicht vorhanden in "Informationssystem/Dienst/Anwendung", aber Pflicht in "Geodatendienst"  -->
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="service-classification"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="publication-scale"/>
        <xs:element minOccurs="0" ref="system-history"/>
        <xs:element minOccurs="0" ref="database-of-system"/>
        <xs:element minOccurs="0" ref="system-environment"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="service-version"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="service-operation"/>
        <xs:element minOccurs="0" ref="has-access-constraint"/>
        <xs:element minOccurs="0" ref="has-atom-download"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="service-url"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Service-Klassifikation (INSPIRE)
    Tabelle.Feld   : T011_obj_serv_type.serv_type_key
    Tabelle.Feld   : T011_obj_serv_type.serv_type_value
  -->
  <xs:element name="service-classification" type="id-string"/>
  <!--
    Fachlicher Name: Art des Dienstes
    Tabelle.Feld   : T011_obj_serv.type_key
    Tabelle.Feld   :T011_obj_serv.type_value
  -->
  <xs:element name="service-type" type="id-string"/>
  <!--
    Fachlicher Name: Kopplungstyp
    Tabelle.Feld   : T011_obj_serv.coupling_type
  -->
  <xs:element name="coupling-type" type="xs:string"/>
  <!--
    Fachlicher Name: Historie
    Tabelle.Feld   : T011_obj_serv.history
  -->
  <xs:element name="system-history" type="xs:string"/>
  <!--
    Fachlicher Name: Basisdaten
    Tabelle.Feld   : T011_obj_serv.base
  -->
  <xs:element name="database-of-system" type="xs:string"/>
  <!--
    Fachlicher Name: Systemumgebung
    Tabelle.Feld   : T011_obj_serv.environment
  -->
  <xs:element name="system-environment" type="xs:string"/>
  <!--
    Fachlicher Name: Zugangsbeschränkungen ("zeige Karte" anzeigen ?)
    Tabelle.Feld   : T011_obj_serv.has_access_constraint
  -->
  <xs:element name="has-access-constraint" default="N" type="igc-boolean"/>
  <!--
    Fachlicher Name: Als ATOM-Download Dienst bereitstellen
    Tabelle.Feld   : T011_obj_serv.has_atom_download
  -->
  <xs:element name="has-atom-download" default="N" type="igc-boolean"/>
  <!--
    Fachlicher Name: URLs von "Informationssystem/Dienst/Anwendung"
    Tabelle   : t011_obj_serv_url
  -->
  <xs:element name="service-url">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name"/>
        <xs:element ref="url"/>
        <xs:element ref="description"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: URL
    Tabelle.Feld   : t011_obj_serv_url.url
  -->
  <xs:element name="url" type="xs:string"/>
  <!--
    Fachlicher Name: Beschreibung von URL
    Tabelle.Feld   : t011_obj_serv_url.description
  -->
  <xs:element name="description" type="xs:string"/>
  <!--
    Fachlicher Name: Version des Service
    Tabelle.Feld   : T011_obj_serv_version.version_key
    Tabelle.Feld   : T011_obj_serv_version.version_value
  -->
  <xs:element name="service-version" type="id-string"/>
  <!--
    Fachlicher Name: Operation
    Tabelle   : T011_obj_serv_op*
  -->
  <xs:element name="service-operation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="operation-name"/>
        <xs:element minOccurs="0" ref="description-of-operation"/>
        <xs:element minOccurs="0" ref="invocation-name"/>
        <xs:element maxOccurs="unbounded" ref="platform"/>
        <xs:element maxOccurs="unbounded" ref="connection-point"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="parameter-of-operation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="depends-on"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Operationbeschreibung
    Tabelle.Feld   : T011_obj_serv_operation.descr
  -->
  <xs:element name="description-of-operation" type="xs:string"/>
  <!--
    Fachlicher Name: Operation-Namensschluessel / -Name
    Tabelle.Feld   : T011_obj_serv_operation.name_key
    Tabelle.Feld   : T011_obj_serv_operation.name_value
  -->
  <xs:element name="operation-name" type="id-string"/>
  <!--
    Fachlicher Name: Aufruf
    Tabelle.Feld   : T011_obj_serv_operation.invocation_name
  -->
  <xs:element name="invocation-name" type="xs:string"/>
  <!--
    Fachlicher Name: Platform
    Tabelle.Feld   : T011_obj_serv_op_platform.platform_key
    Tabelle.Feld   : T011_obj_serv_op_platform.platform_value
  -->
  <xs:element name="platform" type="id-string"/>
  <!--
    Fachlicher Name: Parameter der Operation
    Tabelle   : T011_obj_serv_op_para
  -->
  <xs:element name="parameter-of-operation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name"/>
        <xs:element ref="optional"/>
        <xs:element ref="repeatability"/>
        <xs:element minOccurs="0" ref="direction"/>
        <xs:element minOccurs="0" ref="description-of-parameter"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--
    Fachlicher Name: Attribute des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.optional
  -->
  <xs:element name="optional" type="xs:string"/>
  <!--
    Fachlicher Name: Attribute des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.repeatability
  -->
  <xs:element name="repeatability" type="xs:string"/>
  <!--
    Fachlicher Name: Attribute des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.direction
  -->
  <xs:element name="direction" type="xs:string"/>
  <!--
    Fachlicher Name: Beschreibung des Parameters
    Tabelle.Feld   : T011_obj_serv_op_para.descr
  -->
  <xs:element name="description-of-parameter" type="xs:string"/>
  <!--
    Fachlicher Name: Abhängigkeiten
    Tabelle.Feld   : T011_obj_serv_op_depends.depends_on
  -->
  <xs:element name="depends-on" type="xs:string"/>
  <!--
    Fachlicher Name: Zugriffsadresse
    Tabelle.Feld   : t011_obj_serv_op_connpoint.connect_point
  -->
  <xs:element name="connection-point" type="xs:string"/>
  <!--
    Fachlicher Name: Gutachten / Bericht / Literatur
    Tabelle   : t011_obj_literature
  -->
  <xs:element name="document">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="description-of-tech-domain"/>
        <xs:element minOccurs="0" ref="publisher"/>

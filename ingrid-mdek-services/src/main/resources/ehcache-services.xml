<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <diskStore path="java.io.tmpdir"/>
    <defaultCache
        maxElementsInMemory="10000"
        eternal="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="true"
        diskPersistent="false"
        diskExpiryThreadIntervalSeconds="120"
        />
    
<!-- cache Hashmaps encapsulating SysLists entries: entryId/entryName -->
    <cache name="services-SysListMap"
        maxElementsInMemory="2000"
        eternal="false"
        timeToIdleSeconds="60"
        timeToLiveSeconds="300"
        overflowToDisk="true"
        diskPersistent="false"
        /> 

<!-- cache arbitrary catalog data, e.g. language -->
    <cache name="services-Catalog"
        maxElementsInMemory="2000"
        eternal="false"
        timeToIdleSeconds="60"
        timeToLiveSeconds="600"
        overflowToDisk="true"
        diskPersistent="false"
        />

<!-- cache igc profile -->
    <cache name="services-IGCProfile"
        maxElementsInMemory="2000"
        eternal="false"
        timeToIdleSeconds="60"
        timeToLiveSeconds="120"
        overflowToDisk="true"
        diskPersistent="false"
        /> 

</ehcache>
<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">
   <class name="ObjectAccess" table="object_access">
      <id name="id" type="long" column="id">
         <generator class="hilo"/>
      </id>
      <version name="version" type="integer" column="version"/>
      <property name="objId" type="long" column="obj_id"/>
      <property name="restrictionKey" type="integer" column="restriction_key"/>
      <property name="restrictionValue" type="string" column="restriction_value"/>
      <property name="termsOfUse" type="string" column="terms_of_use"/>

   </class>

</hibernate-mapping>
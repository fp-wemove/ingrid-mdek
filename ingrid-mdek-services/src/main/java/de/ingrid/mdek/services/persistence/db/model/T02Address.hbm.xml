<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">

   <class name="T02Address" table="t02_address">

	<!-- bean superclass -->
      <id name="id" type="long">
         <generator class="hilo"/>
      </id>
      <version name="version" type="integer"/>

	<!-- bean superclass -->
      <property name="catId" type="long" column="cat_id"/>
      <property name="root" type="integer" column="root"/>
      <property name="lastexportTime" type="string" column="lastexport_time"/>
      <property name="expiryTime" type="string" column="expiry_time"/>
      <property name="workState" type="string" column="work_state"/>
      <property name="workVersion" type="integer" column="work_version"/>
      <property name="markDeleted" type="string" column="mark_deleted"/>
      <property name="createTime" type="string" column="create_time"/>
      <property name="modTime" type="string" column="mod_time"/>
      <property name="modId" type="long" column="mod_id"/>
      <property name="responsibleId" type="long" column="responsible_id"/>
      
	<!-- bean class -->
      <property name="adrUuid" type="string" column="adr_uuid"/>
      <property name="orgAdrId" type="string" column="org_adr_id"/>
      <property name="adrType" type="integer" column="adr_type"/>
      <property name="institution" type="string" column="institution"/>
      <property name="lastname" type="string" column="lastname"/>
      <property name="firstname" type="string" column="firstname"/>
      <property name="address" type="string" column="address"/>
      <property name="title" type="string" column="title"/>
      <property name="street" type="string" column="street"/>
      <property name="postcode" type="string" column="postcode"/>
      <property name="postbox" type="string" column="postbox"/>
      <property name="postboxPc" type="string" column="postbox_pc"/>
      <property name="city" type="string" column="city"/>
      <property name="countryCode" type="string" column="country_code"/>
      <property name="job" type="string" column="job"/>
      <property name="descr" type="string" column="descr"/>

      <set name="t021Communications" order-by="line">
         <key column="adr_id"/>
         <one-to-many class="T021Communication" not-found="ignore"/>
      </set>


<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">
   <class name="T08Attr" table="t08_attr">
      <id name="id" type="long" column="id">
         <generator class="hilo"/>
      </id>
      <version name="version" type="integer" column="version"/>
      <property name="attrTypeId" type="long" column="attr_type_id"/>
      <property name="objId" type="long" column="obj_id"/>
      <property name="data" type="string" column="data"/>

      <many-to-one name="t08AttrType" class="T08AttrType" column="attr_type_id" update="false"
                   insert="false"/>
   </class>

</hibernate-mapping>
<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">
   <class name="T0110AvailFormat" table="t0110_avail_format">
      <id name="id" type="long" column="id">
         <generator class="hilo"/>
      </id>
      <version name="version" type="integer" column="version"/>
      <property name="objId" type="long" column="obj_id"/>
      <property name="line" type="integer" column="line"/>
      <property name="name" type="string" column="name"/>
      <property name="ver" type="string" column="ver"/>
      <property name="fileDecompressionTechnique" type="string"
                column="file_decompression_technique"/>
      <property name="specification" type="string" column="specification"/>

   </class>

</hibernate-mapping>
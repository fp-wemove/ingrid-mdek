<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">
   <class name="T08AttrType" table="t08_attr_type">
      <id name="id" type="long" column="id">
         <generator class="hilo"/>
      </id>
      <version name="version" type="integer" column="version"/>
      <property name="name" type="string" column="name"/>
      <property name="length" type="integer" column="length"/>
      <property name="type" type="string" column="type"/>

      <set name="t08AttrLists" order-by="listitem_line, lang_code" table="t08_attr_list"
           inverse="true">
         <key column="attr_type_id"/>
         <one-to-many class="T08AttrList" not-found="ignore"/>
      </set>
   </class>

</hibernate-mapping>
<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">
   <class name="SysGenericKey" table="sys_generic_key">
      <id name="id" type="long" column="id">
         <generator class="hilo"/>
      </id>
      <version name="version" type="integer" column="version"/>
      <property name="keyName" type="string" column="key_name"/>
      <property name="valueString" type="string" column="value_string"/>

   </class>

</hibernate-mapping>
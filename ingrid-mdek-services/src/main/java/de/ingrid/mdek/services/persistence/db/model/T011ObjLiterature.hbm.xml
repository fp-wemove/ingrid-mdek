<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">
   <class name="T011ObjLiterature" table="t011_obj_literature">
      <id name="id" type="long" column="id">
         <generator class="hilo"/>
      </id>
      <version name="version" type="integer" column="version"/>
      <property name="objId" type="long" column="obj_id"/>
      <property name="author" type="string" column="author"/>
      <property name="publisher" type="string" column="publisher"/>
      <property name="typeKey" type="integer" column="type_key"/>
      <property name="typeValue" type="string" column="type_value"/>
      <property name="publishIn" type="string" column="publish_in"/>
      <property name="volume" type="string" column="volume"/>
      <property name="sides" type="string" column="sides"/>
      <property name="publishYear" type="string" column="publish_year"/>
      <property name="publishLoc" type="string" column="publish_loc"/>
      <property name="loc" type="string" column="loc"/>
      <property name="docInfo" type="string" column="doc_info"/>
      <property name="base" type="string" column="base"/>
      <property name="isbn" type="string" column="isbn"/>
      <property name="publishing" type="string" column="publishing"/>
      <property name="description" type="string" column="description"/>

   </class>

</hibernate-mapping>
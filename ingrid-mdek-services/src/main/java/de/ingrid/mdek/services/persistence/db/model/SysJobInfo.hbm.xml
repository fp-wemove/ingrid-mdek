<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">
   <class name="SysJobInfo" table="sys_job_info">
      <id name="id" type="long" column="id">
         <generator class="hilo"/>
      </id>
      <version name="version" type="integer" column="version"/>
      <property name="jobType" type="string" column="job_type"/>
      <property name="userUuid" type="string" column="user_uuid"/>
      <property name="startTime" type="string" column="start_time"/>
      <property name="endTime" type="string" column="end_time"/>
      <property name="jobDetails" type="string" column="job_details"/>

   </class>

</hibernate-mapping>
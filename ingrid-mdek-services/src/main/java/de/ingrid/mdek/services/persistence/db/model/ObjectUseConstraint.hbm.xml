<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">
   <class name="ObjectUseConstraint" table="object_use_constraint">
      <id name="id" type="long" column="id">
         <generator class="increment"/>
      </id>
      <version name="version" type="integer" column="version"/>
      <property name="objId" type="long" column="obj_id"/>
      <property name="line" type="integer" column="line"/>
      <property name="licenseKey" type="integer" column="license_key"/>
      <property name="licenseValue" type="string" column="license_value"/>
      <property name="source" type="string" column="source"/>

   </class>

</hibernate-mapping>
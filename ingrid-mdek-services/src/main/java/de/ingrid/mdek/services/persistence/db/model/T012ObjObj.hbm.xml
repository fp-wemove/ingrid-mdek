<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.ingrid.mdek.services.persistence.db.model">
   <class name="T012ObjObj" table="t012_obj_obj">
      <id name="id" type="long">
         <generator class="hilo"/>
      </id>
      <version name="version" type="integer"/>

      <property name="objectFromUuid" type="string" column="object_from_uuid"/>
      <property name="objectToUuid" type="string" column="object_to_uuid"/>
      <property name="type" type="integer" column="type"/>
      <property name="line" type="integer" column="line"/>
      <property name="specialRef" type="integer" column="special_ref"/>
      <property name="specialName" type="string" column="special_name"/>
      <property name="descr" type="string" column="descr"/>

	  <!-- NOTICE: NEVER loads lazy although lazy loading requested ! This seems to be
	       because of property-ref, which causes getter call on associated object -->
      <many-to-one name="toT01Object" class="T01Object" column="object_to_uuid" property-ref="objUuid"
			lazy="proxy" update="false" insert="false"/>

   </class>
</hibernate-mapping>